<script language="javascript">

var timerle, up
up = false

function makearray(n){
	this.length = n;
	for(var i = 1; i <= n; i++)
	this[i] = 0;
	return this;
}

hexa = new makearray(16);
for(var i = 0; i < 10; i++) hexa[i] = i;
hexa[10]="a"; hexa[11]="b"; hexa[12]="c";
hexa[13]="d"; hexa[14]="e"; hexa[15]="f";

function hex(i){
	i = Math.floor(i)
	if (i < 0) return "00";
	else if (i >255) return "ff";
	else return "" + hexa[Math.floor(i/16)] + hexa[i%16];
}


function BgFade(col1, col2, steps) {
	red1 = Math.floor(col1 / 0x10000)
	grn1 = Math.floor(col1 % 0x10000) / 0x100
	blu1 = col1 % 0x100

	red2 = Math.floor(col2 / 0x10000)
	grn2 = Math.floor(col2 % 0x10000) / 0x100
	blu2 = col2 % 0x100

	sred = red1; sgrn = grn1; sblu = blu1; 
	ered = red2; egrn = grn2; eblu = blu2; 
	inc = steps; 
	step = 0; 
	timerle = window.setInterval('RunFader()',50); 
}

function Wait() {
	up = !up;
	if (up)
		BgFade(EndCol, StartCol, 20);
	else
		BgFade(StartCol, EndCol, 20);		
}

function RunFader() {
	var epct = step/inc; 
	var spct = 1 - epct; 
	
	var hr = hex(sred * spct + ered * epct); var hg = hex(sgrn * spct + egrn * epct); var hb = hex(sblu * spct + eblu * epct);
	document.getElementById("ddo").style.backgroundColor = "#"+hr+hg+hb
	//document.getElementById("ddo").style.backgroundColor = Math.floor(sred * spct + ered * epct)*256*256 + Math.floor(sgrn * spct + egrn * epct)*256 + Math.floor(sblu * spct + eblu * epct); 
	
	if ( step >= inc ) {
		window.clearInterval(timerle)
		if (!up)
			window.setTimeout('Wait()', 20); 
		else
			window.setTimeout('Wait()', 1000); 
	}
	
	step++;
}

StartCol = 0xEFCA00;
EndCol = 0xFFFF00;

BgFade(StartCol, EndCol, 20);

</script>
