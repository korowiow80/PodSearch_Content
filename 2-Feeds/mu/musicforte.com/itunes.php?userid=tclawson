      <script>
$(function() {
	//initialize first album
	var query = location.href.split('#');
	var anchor = query.length == 2 ? query[1] : '';
	
	// don't need scrolling
	if(!$("#profileAlbums ul > li").length)
	  return;
	
	var albumID = $("#profileAlbums ul > li").eq(0).attr('name').substring(6);

	//check if specific album is selected
	if(anchor.length && $("#album_"+anchor).length)
		albumID = anchor;
	
	//display active album
	showAlbum(albumID);
	$("#profileAlbums ul > li[name=album_"+albumID+"]").addClass("active");
	
	
	//album load (from the main music page)
	$("#profileAlbums ul > li").click(function(){
		showAlbum($(this).attr('name').substring(6));
		return false;
	});
	
	//album load (from left column)
	$(".musicAlbums").click(function(){
		showAlbum($(this).attr('name').substring(6));
		$("#profileAlbums ul > li").removeClass("active");
		$("#profileAlbums ul > li[name=album_"+$(this).attr('name').substring(6)+"]").addClass("active");
		return false;
	});
	
	if($("#profileAlbums ul > li").length < 10)
		$(".nextPage").hide();
});
function showAlbum(item){
	var elem = $("#album_"+item);
	elem.parent().children("li").hide();
	elem.fadeIn();
	window.location = "#"+ item;
}
</script>

