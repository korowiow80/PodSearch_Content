			<script type="text/javascript"> 
			  var mycarousel_itemList = [
			     
                  {url: 'http://www.podfeed.net/images/no_logo.gif', title: 'Smooth Groovers Review', link: 'http://www.podfeed.net/podcast/Smooth+Groovers+Review/3118'}
                  ,
                  {url: 'http://www.podfeed.net/images/no_logo.gif', title: 'Sciencecast.net', link: 'http://www.podfeed.net/podcast/Sciencecast.net/1169'}
                  ,
                  {url: 'http://www.podfeed.net/images/no_logo.gif', title: 'Speechless:Exploring New Instrumental Music', link: 'http://www.podfeed.net/podcast/SpeechlessExploring+New+Instrumental+Music/1302'}
                  ,
                  {url: 'http://www.podfeed.net/images/no_logo.gif', title: 'Ockhams Razor', link: 'http://www.podfeed.net/podcast/Ockhams+Razor/1889'}
                  ,
                  {url: 'http://www.podfeed.net/images/no_logo.gif', title: 'The Kedou Chronicles', link: 'http://www.podfeed.net/podcast/The+Kedou+Chronicles/1974'}
                  ,
                  {url: 'http://www.geocities.com/skepticradio/images/moment3.JPG', title: 'A Skeptic Moment with Tim Kammer', link: 'http://www.podfeed.net/podcast/A+Skeptic+Moment+with+Tim+Kammer/1991'}
                  ,
                  {url: 'http://www.podfeed.net/images/no_logo.gif', title: '- The Inoculated Mind - Mindcast', link: 'http://www.podfeed.net/podcast/-+The+Inoculated+Mind+-+Mindcast/2006'}
                  ,
                  {url: 'http://www.podfeed.net/images/no_logo.gif', title: 'Intellectual Icebergs', link: 'http://www.podfeed.net/podcast/Intellectual+Icebergs/2445'}
                  
            ];
             
            function mycarousel_itemVisibleInCallback(carousel, item, i, state, evt)
            {
                // The index() method calculates the index from a
                // given index who is out of the actual item range.
                var idx = carousel.index(i, mycarousel_itemList.length);
                carousel.add(i, mycarousel_getItemHTML(mycarousel_itemList[idx - 1]));
            };
             
            function mycarousel_itemVisibleOutCallback(carousel, item, i, state, evt)
            {
                carousel.remove(i);
            };
             
            /**
             * Item html creation helper.
             */
            function mycarousel_getItemHTML(item)
            {
                return '<a href="' + item.link + '"><img src="' + item.url + '" onerror="this.src=\'http://www.podfeed.net/images/no_logo.gif\'" width="125" height="125" border="0" alt="' + item.title + '" /></a><div>' + item.title + '</div>';
            };
             
            jQuery(document).ready(function() {
                jQuery('#mycarousel').jcarousel({
                    wrap: 'circular',
                    itemVisibleInCallback: {onBeforeAnimation: mycarousel_itemVisibleInCallback},
                    itemVisibleOutCallback: {onAfterAnimation: mycarousel_itemVisibleOutCallback}
                });
            });
       
      </script> 
			
			
