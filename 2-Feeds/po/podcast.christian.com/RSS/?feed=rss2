<link rel="stylesheet" type="text/css" href="/themes/default/images/ui.datepicker.css" /><script type="text/javascript" src="/public/js/jquery.ui.core.min.js"></script><script type="text/javascript" src="/public/js/jquery.ui.datepicker.min.js"></script>    
<script language="javascript">
			var delay = 5000;
      var count = 30;
      var showing = 5;
      var i = 0;
      function move(i) {
        return function() {
          $('#recent'+i).remove().css('display', 'none').prependTo('#items');
        }
      }
      function shift() {
        var toShow = (i + showing) % count;
        $('#recent'+toShow).slideDown(1000, move(i));
        $('#recent'+i).slideUp(1000, move(i));
        i = (i + 1) % count;
        setTimeout('shift()', delay);
      }


$(function() {
	shift(); 
	showtext_p($("#reg"), $("#pwd-password"), "Password");
	//showtext_p($("#reg"), $("#pwd-password1"), "Confirm Password");
	$('#text-birthday').datepicker({
			dateFormat:'MM d, yy',
			//showOn: 'button',
			//buttonImageOnly: true,
			changeMonth: true,
			changeYear: true,
			maxDate: 'm w',
			buttonImage: '/public/images/calendar.gif',
			yearRange:'1930:2012'
	}).attr({readonly:'true'});
});
function showtext_p(form, element, text) {
    var phony = $("<input>");
    phony.attr("type", "text");
    phony.attr("id", element.attr("id"));
    phony.attr("class","reginput");
    phony.val(text);
    phony.insertAfter(element);
    element.hide();
    phony.focus(function() { phony.hide(); element.show(); element.focus(); });
    element.blur(function() { if (!element.val()) { element.hide(); phony.show(); } });
}
</script>

