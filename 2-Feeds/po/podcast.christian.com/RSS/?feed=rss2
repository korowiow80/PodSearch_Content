<script type="text/javascript">
	
var ajaxImage = ['/public/images/ajaxloading.gif','/public/images/ok.gif','/public/images/error.gif'];
var submitvalue = '';
	
$(function(){
	
	if($('#resultAjax').length==0){
		$(document.body).append('<div id="resultAjax" style="position: absolute;right:10px;top:10px;"></div>');
	}
	
	if($('#reginfo').length==0){
		$(document.body).append('<div id="reginfo" style="position: absolute;right:10px;top:10px;"></div>');
	}
	
	var optionsreg = {
		//target:     '#result',
		//url:        'comment.php',
		dataType : 'json',
		success: showResponsereg
	};
	$('#reg').submit(function() {
		$('#reginfo').html('<IMG SRC="'+ajaxImage[0]+'"  BORDER="0" ALT="loading..." align="absmiddle"> Loading Data, Please wait...');
		$('#reginfo').fadeTo("slow",0.85);
		$('input:submit',this).attr('disabled','true');
		submitvalue = $('input:submit',this).val();
		//$('input:submit',this).val('Submiting, Please wait...');
		$(this).append('<input type="hidden" name="ajax" value="1">');
		$(this).ajaxSubmit(optionsreg);
		return false;
	});
	
});
function showResponsereg(responseText, statusText)  {
    if(responseText.status==1){
    	if(responseText.info!='') $('#reginfo').html('<IMG SRC="'+ajaxImage[1]+'" BORDER="0" ALT="success..." align="absmiddle"> <span style="color:blue">'+responseText.info+'</span>');
    	
    	if($('#hidden-jumpUrl',$('#reg')).val()!=undefined){
    		setInterval("window.location.href=$('#hidden-jumpUrl',$('#reg')).val();",2500);
    	}else{
    		setInterval("window.location.reload()",2500);
    	}
    }else{
    	$('#reginfo').html('<IMG SRC="'+ajaxImage[2]+'" BORDER="0" ALT="error..." align="absmiddle"> <span style="color:red">'+responseText.info+'</span>');
    	$('input:submit',$('#reg')).attr('disabled',''); 
			//$('input:submit',$('#reg')).val(submitvalue);
			$('#reginfo').fadeOut(2500);
    }
}

</script>
	                 
