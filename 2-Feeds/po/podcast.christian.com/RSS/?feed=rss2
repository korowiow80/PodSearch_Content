<script language="javascript">
$(function() { 
	showtext_password($("#login"), $("#pwd-pwd"), "Password");
	$("#whatis").mouseover(function() {
		$("#hide_whatis").slideDown();
	});
	$("#whatis").mouseout(function() {
		$("#hide_whatis").slideUp();
	});
});
function showtext_password(form, element, text) {
    var phony = $("<input>");
    phony.attr("type", "text");
    phony.attr("id", element.attr("id"));
    phony.attr("style","padding-left:2px;height:18px;width:110px");
    phony.val(text);
    phony.insertAfter(element);
    element.hide();

    phony.focus(function() { phony.hide(); element.show(); element.focus(); });
    element.blur(function() { if (!element.val()) { element.hide(); phony.show(); } });
}
</script>
    
