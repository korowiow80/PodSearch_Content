		<script type="text/javascript">
			//function adjust layout set hieght max
			function adjustLayout(){
				// Get natural heights
				var cHeight = $("#main").height();
				var lHeight = $("#left").height();
				var rHeight = $("#r_menu").height();
				// Find the maximum height
				var maxHeight = Math.max(cHeight, Math.max(lHeight, rHeight));
				// Assign maximum height to all columns
				$("#main").css('height', maxHeight);
				$("#left").css('height',maxHeight);
				$("#r_menu").css('height', maxHeight); 
	
			} 
			 window.onload = function(){
				var cHeight = $("#main").height();
				var lHeight = $("#left").height();
				var rHeight = $("#r_menu").height();
				if(lHeight > cHeight || rHeight > cHeight){
					adjustLayout();
				}
			} 
		</script>
