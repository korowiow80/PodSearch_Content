data[1] ="Bremen|Gewitter|15|10|80|";
data[2] ="Dresden|Regenschauer|18|11|30|";
data[3] ="D&uuml;sseldorf|Gewitter|18|13|90|";
data[4] ="Erfurt|Regenschauer|16|9|80|";
data[5] ="Frankfurt|Regenschauer|18|11|90|";
data[6] ="Hamburg|Regenschauer|16|11|90|";
data[7] ="Hannover|Gewitter|16|9|80|";
data[8] ="Kiel|Regenschauer|15|10|50|";
data[9] ="K&ouml;ln|Gewitter|18|13|90|";
data[10] ="Leipzig|Regenschauer|18|10|50|";
data[11] ="Magdeburg|Regenschauer|17|10|90|";
data[12] ="M&uuml;nchen|Regenschauer|20|10|50|";
data[13] ="N&uuml;rnberg|Regenschauer|18|9|70|";
data[14] ="Saarbr&uuml;cken|Regenschauer|19|11|90|";
data[15] ="Schwerin|Regenschauer|16|8|50|";
data[16] ="Stuttgart|Regenschauer|20|11|70|";


var aktiv2 = window.setInterval("MyWeather()", 3000);

var j1 = 0;
var iDay = 0;

SetWeather(data[0]);

function SetWeather(dline) {

	var dedata = dline.split('|');

	document.getElementById('mw_name').innerHTML=dedata[0];

	var wetter = dedata[1];
	document.getElementById('mw_wetter').innerHTML="<b>"+wetter+"</b>";
	document.getElementById('mw_temp').innerHTML="<b>"+dedata[2]+"&deg\;C</b>";
	document.getElementById('mw_icon').src='http://wetter.welt.de/symbols/45x45/'+dedata[1]+'.png';

}

function MyWeather() {

	j1 = j1 + 1;
	if (j1 > 16) j1=0;

	SetWeather(data[j1]);

}

function Go (select) {

	if (select == "Ort oder Postleitzahl") {
		alert("BITTE PLZ/ORT EINGEBEN");
	}
	else if (select == "") {
		alert("BITTE PLZ/ORT EINGEBEN");
	}
	else
	{

		var search = "?s="+select;
		top.location.href = "http://wetter.welt.de/suche.asp" + search;
    }
}



</script>
</div>
</div>
