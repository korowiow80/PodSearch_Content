					event.preventDefault();
					
					var	action	= $( this ).attr( 'data-action' );
				
					switch ( action )
					{
						case 'close':
							var	target	= $( this ).attr( 'data-target' );
							$( target ).fadeOut();
						break;
						
						case 'toggle':
							var	target	= $( this ).attr( 'data-target' );
							
							if  ( ( target == '.top-slider-container' ) && ( ! $( target ).is( ':visible' ) ) )
							{
								//AudioStreamNew();							
							}
							
							$( target ).slideToggle();
						break;
						
						case 'show-hint':					
							var	target	= $( this ).attr( 'data-target' ); 	// WHERE is the new feture
							var	hint	= $( this ).attr( 'data-hint' );	// Hint selector
							
							$( hint ).show();
							
							$( 'html').scrollTop( 0 );
							
							element_pulsate( hint );
							
						break;
					}
				});
				
				$(".hint-box").hover
				(
					function () 
					{
						$( this ).find( '.hint-box-close' ).show();
					},
					function ()
					{
						$( this ).find( '.hint-box-close' ).hide();
					}
				);
				
			});
