	<script type="text/javascript">

		function createRequestObject() {
			var ro;
			var browser = navigator.appName;
			if(browser == "Microsoft Internet Explorer"){
				ro = new ActiveXObject("Microsoft.XMLHTTP");
			}else{
				ro = new XMLHttpRequest();
			}
			return ro;
		}

		var http = createRequestObject();

		function sndReq(action) {
			http.open('get', 'ajax_insert.asp?action='+action);
			http.onreadystatechange = handleResponse;
			http.send(null);
		}

		function handleResponse() {
			if(http.readyState == 4){
				var response = http.responseText;
			}
		}

		function objectToggle(objectName) {
			$(objectName).toggle();
			return false;
		}
		
		function effectFadeIn(classname) {
			$("."+classname).fadeOut(3000).fadeIn(3000, effectFadeOut(classname))
		}
		
		function effectFadeOut(classname) {
			$("."+classname).fadeIn(3000).fadeOut(3000, effectFadeIn(classname))
		}

		function element_pulsate( selector )
		{
			var p = $( selector );
			for ( var i=0; i<3; i++ )
			{
				p.animate( {opacity: 0.2}, 700, 'linear' ).animate( {opacity: 1}, 700, 'linear' );
			}		
		}
		
		// Load jQuery-functions after document is loaded
		$(document).ready(function(){
			//effectFadeIn('band');
			
			$('#ticker ul.rotate').list_ticker({
				speed:	4000,
				effect:	'fade'
			});					
			
			$('.expand-radio').click(function(event) {
				event.preventDefault();
				
				$( '#hint-1' ).show();
				$( 'html').scrollTop( 0 );
				element_pulsate( '#hint-1' );		
				//$( '#hint-1 span.hint-box-pointer' ).css( 'background-image' , 'url("http://static.domradio.org/public/images/hintbox_pointer_top.png")' );
				//AudioStreamNew();
				//$('.expander-radio').toggle();
			});
			
			$('.expand').click(function(event) {
				event.preventDefault();
				$('.expander').parent().find('ul.expander').toggle();
			});			
			
			$("a.show-overlay").click(function(event){
				event.preventDefault();
				$("#overlay").fadeIn("slow");
				$("#overlay-back").css("display", "block");
				$(".hide-overlay").css("display", "block");
				sndReq('up');
			});

			$("a.hide-overlay").click(function(event){
				event.preventDefault();
				$("#overlay").fadeOut("slow");
				$("#overlay-back").css("display", "none");
				$(".hide-overlay").css("display", "none");
			});
		
		});
	</script>

