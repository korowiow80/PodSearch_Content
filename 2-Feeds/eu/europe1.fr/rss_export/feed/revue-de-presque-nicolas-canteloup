<script type="text/javascript">
    var radarSettings = {
        zoneId: '1',
        customerId: '10399'
    };
    (function(d, w) {
        var onWindowLoaded = function() {
            var a = document.createElement('script');
            a.type = 'text/javascript';
            a.src = "http://radar.cedexis.com/01-10399-radar10.min.js";
            d.body.appendChild(a);
        };
        if (w.location.protocol == 'https:') { return; }
        if (w.addEventListener) { w.addEventListener('load', onWindowLoaded, false); }
        else if (w.attachEvent) { w.attachEvent('onload', onWindowLoaded ); }
    })(document, window);
</script>


<link rel="stylesheet" href="/includes/cobrand/css/jquery-ui-1.8.20.custom-forautocomplete.css" />

<script type="text/javascript" src="/includes/cobrand/js/jquery.ui.core.min.js"></script>
<script type="text/javascript" src="/includes/cobrand/js/jquery.ui.widget.min.js"></script>
<script type="text/javascript" src="/includes/cobrand/js/jquery.ui.autocomplete.min.js"></script>
<script type="text/javascript" src="/includes/cobrand/js/jquery.ui.position.min.js"></script>
<script type="text/javascript">
	function initAutocompleteSearch() {
	    $( "#field_candSs" ).autocomplete({
	           source: function( request, response ) {
	                   $.ajax({
	                           url: "http://elections.europe1.fr/json",
	                           dataType: "jsonp",
	                           data: {
	                                  featureClass: "P",
	                                  style: "full",
	                                  maxRows: 12,
	                                  candSs: request.term
	                           },
	                           success: function( data ) 
	           				{
	           					var list = data;
	           					response( $.map( list, function( item ) {
	           						if (item.deputename) {
	           							return {
	           								label: item.namecand + ' (député sortant)',
	           								value: item.namecand
	           							}
	           						} else {
	           							return {
	           								label: item.namecand,
	           								value: item.namecand 
	           							}
	           						}
	           					}));
	           				}
	           			});
	           		},
	           		minLength: 4,
	           		select: function( event, ui ) {
	           			$("#field_candSs").val(ui.item.value);
	           		}
	    });
	}
	initAutocompleteSearch();
</script>



