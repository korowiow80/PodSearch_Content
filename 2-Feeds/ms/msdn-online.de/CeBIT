            scriptElem.async = true;
            scriptElem.type = 'text/javascript';
            scriptElem.src = 'http://Ads1.msn.com/library/dap.js';
            if (scriptElem.addEventListener) {
                scriptElem.addEventListener("load", renderAds, false);
            } else if (scriptElem.readyState) {
                scriptElem.onreadystatechange = function () {
                    if (this.readyState == 'complete' || this.readyState == 'loaded') renderAds();
                };
            }
            document.getElementsByTagName('head')[0].appendChild(scriptElem);
            dapjsloaded=true;
            }

            function renderAds() {
                while(adsRendered.length != 0){
                var ads = adsRendered.pop();
