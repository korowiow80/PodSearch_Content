                        dapMgr.enableACB(ads.uniqueId, false);
                        dapMgr.renderAd(ads.uniqueId, "&amp;PG=" + ads.adCode + "&amp;AP=" + ads.size, ads.width, ads.height);
                        var uniqueClientId = document.getElementById(ads.uniqueId + "leader");
                        uniqueClientId.style.width = ads.width;
                    } catch (ex) { }
                }
            };
