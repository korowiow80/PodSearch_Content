            <pubDate>Sun, 06 Jul 2008 16:47:00 +0000</pubDate>
            <itunes:explicit>clean</itunes:explicit>
			<itunes:keywords>new,business,public,policy,jersey,industry,lubetkin,njpro,njbia</itunes:keywords>
