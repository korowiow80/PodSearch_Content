 <category domain="http://www.jurtenland.de/taxonomy/term/399">schlaufenjurte</category>
 <category domain="http://www.jurtenland.de/category/tags/schlingen">schlingen</category>
 <pubDate>Wed, 11 Apr 2012 13:59:13 +0000</pubDate>
 <dc:creator>ralph</dc:creator>
