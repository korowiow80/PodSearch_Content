	<script type="text/javascript">/* <![CDATA[ */

				function createCookie(name,value,days) {
					if (days) {
						var date = new Date();
						date.setTime(date.getTime()+(days*60*60*1000));
						var expires = '; expires='+date.toGMTString();
					} else var expires = '';
					document.cookie = name+'='+value+expires+'; path=/';
				}

				function readCookie(name) {
					var nameEQ = name + '=';
					var ca = document.cookie.split(';');
					for(var i=0;i < ca.length;i++) {
						var c = ca[i];
						while (c.charAt(0)==' ') c = c.substring(1,c.length);
						if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
					}
					return null;
				}

			 	function menue_read() {
					var container = readCookie('navigation');
					menue(container);
				}

			  	function menue(menue_name){
			document.getElementById('m2_5045').style.display = "none";
document.getElementById('m5_5045').style.display = "none";


			  		if (menue_name != null && menue_name != 'null' && menue_name != 'm0') {
						document.getElementById(menue_name).style.display='block';

					}

					 createCookie('navigation',menue_name,1);
			  	}

			  	function activetop(toplevel, read){

			document.getElementById('toplevel1_5045').className = 'menue_point_toplevel_5045';
document.getElementById('toplevel2_5045').className = 'menue_point_toplevel_5045';
document.getElementById('toplevel3_5045').className = 'menue_point_toplevel_5045';
document.getElementById('toplevel4_5045').className = 'menue_point_toplevel_5045';
document.getElementById('toplevel5_5045').className = 'menue_point_toplevel_5045';
document.getElementById('toplevel6_5045').className = 'menue_point_toplevel_5045';
document.getElementById('toplevel7_5045').className = 'menue_point_toplevel_5045';
document.getElementById('toplevel8_5045').className = 'menue_point_toplevel_5045';
document.getElementById('toplevel9_5045').className = 'menue_point_toplevel_5045';

					document.getElementById(toplevel).className=document.getElementById(toplevel).className+'_over';

					createCookie('activetop',toplevel,1);

					if (read != 'read') {
						createCookie('activesecond','0',1);
					}
				}

				function activesecond(secondlevel){
					document.getElementById(secondlevel).className=document.getElementById(secondlevel).className+'_over';
					createCookie('activesecond',secondlevel,1);
				}

				function activetop_read(toplevel) {
					var toplevel = readCookie('activetop');

					if (toplevel == null) {
			toplevel = 'toplevel1_5045';
					}

					activetop(toplevel,'read');
				}

				function activesecond_read(secondlevel) {
					var secondlevel = readCookie('activesecond');
					if (secondlevel != '0' && secondlevel != null && secondlevel != 'null') {
						activesecond(secondlevel);
					}
				}
			/* ]]> */</script>


