   // AJAX redirect
   if ((document.URL.indexOf('#') == -1)
      && (document.URL.indexOf('_escaped_fragment_') == -1)
      && (document.URL.indexOf('vv') > 0)
      && (document.URL.indexOf('content') > 0)) {
      var protocol   = 'http';
      var serverName = 'www.dasding.de';

      if((protocol.length > 0) && (serverName.length > 0) && (document.URL.indexOf(serverName) > 0)) {
         var dasdingBase = protocol + "://" + serverName; // dasding base url
         var port        = '80';
         var prefix      = '';

         if ((port.length > 0) && (port != '80'))
            dasdingBase = dasdingBase + ':' + port;

         if ( prefix.length > 0 )
            dasdingBase = dasdingBase + prefix + '/';

         document.location.href = dasdingBase + '#!' + document.URL;
      }
   } else {
      // Set html title tag directly
      var html_detail_title = 'HOME';
      var html_title = 'HOME /// DASDING.de';

      if (html_detail_title == "") html_detail_title = html_title;

      top.document.getElementsByTagName('title')[0].text = escapeApostrophes(html_detail_title);
      top.document.title = escapeApostrophes(html_title);

      var links = document.getElementsByTagName('link');
      for (i=0; i < links.length; i++) {
         if(links[i].getAttribute('rel') == 'canonical'){
            links[i].setAttribute('href', 'http://www.dasding.de/home/-/id=236/1tqw1w7/index.html');
         }
         if(links[i].getAttribute('rel') == 'image_src'){
            links[i].parentNode.removeChild(links[i--]);
         }
      }

      // Set meta tag description and keywords
      var html_description = 'DASDING - Live.Laut.Lässig. Das junge Programm für Musik und Lifestyle vom SWR.';

      var metaContents = document.getElementsByTagName('meta');
      for (i=0; i < metaContents.length; i++) {
         if(metaContents[i].getAttribute('name') == 'description'){
            metaContents[i].setAttribute('content', escapeApostrophes(html_description));
         }
         if(metaContents[i].getAttribute('name') == 'keywords'){
            metaContents[i].setAttribute('content', 'Sendungen,Home,DASDING,Livestream,Frequenzen,das ding,Videos,DASDING.tv,Playlist,onDemand,Radio,TV,RSS');
         }
          if(metaContents[i].getAttribute('name') == 'x-content-href'){
             metaContents[i].setAttribute('content', 'http://www.dasding.de/home/-/id=236/vv=content/87c8xx/index.html');
          }

         if(metaContents[i].getAttribute('property') == 'og:type'){
            metaContents[i].parentNode.removeChild(metaContents[i--]);
         }
         if(metaContents[i].getAttribute('property') == 'og:video'){
            metaContents[i].parentNode.removeChild(metaContents[i--]);
         }
         if(metaContents[i].getAttribute('property') == 'og:video:type'){
            metaContents[i].parentNode.removeChild(metaContents[i--]);
         }
         if(metaContents[i].getAttribute('property') == 'og:video:width'){
            metaContents[i].parentNode.removeChild(metaContents[i--]);
         }
         if(metaContents[i].getAttribute('property') == 'og:video:height'){
            metaContents[i].parentNode.removeChild(metaContents[i--]);
         }
         if(metaContents[i].getAttribute('property') == 'og:image'){
            metaContents[i].parentNode.removeChild(metaContents[i--]);
         }
         if(metaContents[i].getAttribute('name') == 'product-image'){
            metaContents[i].parentNode.removeChild(metaContents[i--]);
         }
         if(metaContents[i].getAttribute('property') == 'og:title'){
            metaContents[i].setAttribute('content', escapeApostrophes(html_detail_title));
         }
         if(metaContents[i].getAttribute('property') == 'og:url'){
            metaContents[i].setAttribute('content', 'http://www.dasding.de/home/-/id=236/1tqw1w7/index.html');
         }
         if(metaContents[i].getAttribute('property') == 'og:description'){
            metaContents[i].setAttribute('content', escapeApostrophes(html_description));
         }
      }
      // OG-Meta Tags setzen
      $$('head').first().insert({
          bottom: '<meta property="og:type" content="website" />\n\t<meta property="og:image" content="http://www.dasding.de/static/dasding.gif" />\n\t<meta name="product-image" content="http://www.dasding.de/static/dasding_188x138.jpg" />\n\t<link rel="image_src" href="http://www.dasding.de/static/dasding.gif" type="URI"/>\n\t'
