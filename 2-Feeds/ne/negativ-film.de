word-wrap:break-word;
clear:left;
float:left;
font-family:sans-serif;
font-weight:bold;
line-height:140%;
padding:2px;
margin-top:4px;
text-align:left;
display:block;
width:100%;
}
.nrelate_default .nrelate_logo {
background: none !important;
float:left;
clear: both;
padding: 5px 5px 10px 5px;
border:none !important;
width:auto !important;
-moz-box-shadow: none !important;
-webkit-box-shadow: none !important;
-khtml-box-shadow: none !important;
box-shadow: none !important;
}
.nrelate_default .nrelate_logo:hover {
-moz-opacity: .8;
opacity: .8;
-khtml-opacity: .8;
}
.nrelate_default .nr_rc_source {
font-size: 9px;
font-style: italic;
font-weight:normal;
}
.nrelate_default .nr_rc_panel .nr_rc_post_title,
.nrelate_default .nr_rc_panel .nr_rc_source,
.nrelate_default .nr_rc_panel .nr_excerpt{
display:block;
}
.nrelate_default .nr_rc_panel .nr_excerpt{
font-weight:normal;
}
.nrelate_default ul li .nr_rc_link,
.nrelate_default ul li .nr_rc_link:hover,
.nrelate_default ul li .nr_rc_link:active,
.nrelate_default ul li .nr_rc_link:link{
border:none;
}
.nrelate_default ul li .nr_excerpt,
.nrelate_default ul li .nr_rc_source{
padding-left:5px;
}
/* Sponsored area on thumbnails */
.nrelate_default .nr_sponsored_wrap{
display:block;
height:18px;
position:absolute;
left:3px;
overflow:hidden;
}
.nrelate_default  a.nr_rc_panel:hover .nr_sponsored_wrap{
left:3px;
}
/* settings for each thumbnail size supported in plugin */
.nrelate_default.nr_80  .nr_rc_panel{ width : 88px; }
.nrelate_default.nr_80  .nr_rc_panel .nr_sponsored_wrap{ width : 80px; top : 65px; }
.nrelate_default.nr_80  .nr_rc_panel .nr_sponsored{ left : 62px; }
.nrelate_default.nr_90  .nr_rc_panel{ width : 98px; }
.nrelate_default.nr_90  .nr_rc_panel .nr_sponsored_wrap{ width : 90px; top : 75px; }
.nrelate_default.nr_90  .nr_rc_panel .nr_sponsored{ left : 72px; }
.nrelate_default.nr_100 .nr_rc_panel{ width : 108px; }
.nrelate_default.nr_100 .nr_rc_panel .nr_sponsored_wrap{ width : 100px; top : 85px; }
.nrelate_default.nr_100 .nr_rc_panel .nr_sponsored{ left : 82px; }
.nrelate_default.nr_110 .nr_rc_panel,
.nrelate_default.nr_ .nr_rc_panel{ width: 118px; }
.nrelate_default.nr_110 .nr_rc_panel .nr_sponsored_wrap,
.nrelate_default.nr_ .nr_rc_panel .nr_sponsored_wrap{ width : 110px; top : 95px; }
.nrelate_default.nr_110 .nr_rc_panel .nr_sponsored,
.nrelate_default.nr_ .nr_rc_panel .nr_sponsored{ left : 92px; }
.nrelate_default.nr_120 .nr_rc_panel{ width : 128px; }
.nrelate_default.nr_120 .nr_rc_panel .nr_sponsored_wrap{ width : 120px; top : 105px; }
.nrelate_default.nr_120 .nr_rc_panel .nr_sponsored{ left : 102px; }
.nrelate_default.nr_130 .nr_rc_panel{ width : 138px; }
.nrelate_default.nr_130 .nr_rc_panel .nr_sponsored_wrap{ width : 130px; top : 115px; }
.nrelate_default.nr_130 .nr_rc_panel .nr_sponsored{ left : 112px; }
.nrelate_default.nr_140 .nr_rc_panel{ width : 148px; }
.nrelate_default.nr_140 .nr_rc_panel .nr_sponsored_wrap{ width : 140px; top : 125px; }
.nrelate_default.nr_140 .nr_rc_panel .nr_sponsored{ left : 122px; }
.nrelate_default.nr_150 .nr_rc_panel{ width : 158px; }
.nrelate_default.nr_150 .nr_rc_panel .nr_sponsored_wrap{ width : 150px; top:135px; }
