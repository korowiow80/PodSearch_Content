            function myCheckerNoCIS() {
                document.checker.onLoad = null;
                document.checker.onError = null;
            }
            function myCheckerStartCISpopUp(pUrl, pLngPage, pRefererID, pWidth, pHeight, pTop, pLeft) {
                var refererID = pRefererID;
                var SeitennameSprache = pLngPage;
                var width = (pWidth) ? pWidth : "660";
                var height = (pHeight) ? pHeight : "650";
                var screenx = (pTop) ? pTop : "20";
                var top = (pTop) ? pTop : "20";
                var screeny = (pLeft) ? pLeft : "100";
                var left = (pLeft) ? pLeft : "100";
                var url = (pUrl.substr(-1, 1) == "/") ? pUrl : pUrl + "/";
                var FbAufruf = url + SeitennameSprache + ".php?refererID=" + refererID;
                open(FbAufruf, "CISWindow", "resizable,scrollbars,width=" + width + ",height=" + height + ",screenx=" + screenx + ",screeny=" + screeny + ",top=" + top + ",left=" + left);
            }
            function myCheckerStartCISdiv(divID) {
                var elem = document.getElementById(divID);
                elem.style.visibility = "visible";
            }
            function closeDiv(divID) {
                var elem = document.getElementById(divID);
                elem.style.visibility = "hidden";
            }</script>
