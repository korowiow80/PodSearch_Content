<script type="text/javascript" language="javascript">
	jQ(".cookie_tag").click(
	
		function () {
			var overlay_html = jQ(jQ(this).attr("href")).html();
			var overlay_content = new OTT.UI.Overlay_content(overlay_html);
			OTT.UI.Overlay.show(overlay_content);
			
			if(getOptCookie() == "no") {
				$('div.overlay_content_wrap div.cookie_info form').find("[name='opt_in']").removeAttr("checked");
			}
			jQ("div.overlay_content_wrap div.cookie_info form .cancel").click(function() {
				OTT.UI.Overlay.hide();
			})
			
			jQ("div.overlay_content_wrap div.cookie_info form").submit(function() {
				allow = ($(this).find("[name='opt_in']").attr("checked")) ? "yes" : "no";
				
				writeToStorage(allow);
			});
			
			jQ(".cookie_info").parent().parent().css('width', '560px').css('padding','0px');
			jQ(".cookie_info").parent().parent().parent('.overlay_positioner').css('top','10%').css('width', 'auto');
	
			return false;
		});

</script>
	
	
	
	
	
	
	
	
	
	
	

	

	

	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
