	function trackBotSearchSubmit(){
		_gaq.push(['_trackEvent', 'FloatingBottomSearch', 'Submit', '']);
	}
	function trackBotSearchDontShow(){
		_gaq.push(['_trackEvent', 'FloatingBottomSearch', 'DontShow', '']);
	}
	function trackBotSearchClose(){
		_gaq.push(['_trackEvent', 'FloatingBottomSearch', 'Close', '']);
