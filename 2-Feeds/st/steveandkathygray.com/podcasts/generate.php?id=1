  <script type="text/javascript">
	
		function waitonclick() { 
   			var thisClass = $('.roundabout-in-focus').attr('class').split(' ')[0];
			$('span.' + thisClass).fadeIn('fast').addClass('current-p');	
   		}
   	
	
		$(document).ready(function(){
			
			$("label.email, label.name").inFieldLabels();
		
			$('.tabs .right').click(function(){
				$('#contact-slide').fadeIn('slow');
				$('#press-slide').fadeOut('slow');
			});
			$('.tabs .left').click(function(){
				$('#contact-slide').fadeOut('slow');
				$('#press-slide').fadeIn('slow');
			});
					
			//page dropdown	
			$('.tabs .right, .tabs .left').click(function(){
				$('#wrapper').animate({"top": "131px"}, 300).addClass('tabs-open');
				$('.tabs .slide-up').fadeIn('slow');
			});
			
			$('.tabs .slide-up').click(function(){
				$('#wrapper').animate({"top": "0px"}, 300).removeClass('tabs-open');
				$(this).fadeOut('slow');
			})
				
				/*if($('#wrapper').hasClass('tabs-open')){
					$('#wrapper').animate({"top": "0px"}, 300).removeClass('tabs-open');
				}
				else {
					$('#wrapper').animate({"top": "131px"}, 300).addClass('tabs-open');
				}*/
			
			
			
			//$('h3.question.open').click(function(){ return false; });
		
			$('h3.question').click(function(){
				$('.drop-down-area.open').slideUp();
				
				if ($(this).hasClass('open')){
					$('#site-container-lower').animate({"height" : "380px"}, 'slow', function(){
						$('#site-container-lower *').removeClass('open');
						return false;
					});
				}
				else {
					$('#site-container-lower *').removeClass('open');
					$('#site-container-lower').animate({"height" : "700px"}, 'fast');
					$(this).addClass('open').next('div.drop-down-area').slideDown().addClass('open');		
				}
			});
			
			
			//roundabouts
			$('ul.ministries').roundabout({
				minOpacity: 0,
				
				clickToFocusCallback: function(){
					$('span.current-p').removeClass('current-p').fadeOut('fast', function(){
						waitonclick();
					});
				},
			});
   			
   			
   		});
	
	</script>

