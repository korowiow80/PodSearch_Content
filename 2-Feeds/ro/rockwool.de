			function searching(id)
			{
				if(id=='qs_applications')
				{
					searching_applications();
				}
				else
				{
					searching_products();
				}
			}
			function searching_products()
			{
				var v = document.quick_search.qs_products.value;
				if(keywords_urls[v])
				{
					if (keywords_urls[v].indexOf('http://') == -1) {
						document.location.href= 'http://produktwegweiser-rocknavi.rockwool.de'+ keywords_urls[v];
					} else {
						document.location.href= keywords_urls[v];
					}
					return false;
				}
				return true;
			}
			function searching_applications()
			{
				var v = document.qs_applications_form.qs_applications.value;
				if(keywords_applications_urls[v])
				{
					document.location.href= 'http://produktwegweiser-rocknavi.rockwool.de'+ keywords_applications_urls[v];
					return false;
				}
