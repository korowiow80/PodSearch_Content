<html>
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
		<script type="text/javascript" language="JavaScript" src="http://sedoparking.com/registrar/dopark.js"></script>
	</head>
	<body style="display: none;">
		<script type="text/javascript">
			function getParam() { 
				var query = window.location.search.substring(1); 
				var vars = query.split("&"); 
				for (var i=0;i<vars.length;i++) {
			    	var pair = vars[i].split("=");
			    	if (pair[0] == 'domain') {
			      		return pair[1];
			    	}
			  	}
			  	return  window.location.host;
			};
			
			var reg = 'sedopark',
				domain = getParam();
			
			document.write( '<scr'+'ipt type="text/javascript" language="JavaScript" src="http://sedoparking.com/' + domain + '/' + reg + '/park.js"><\/scr' + 'ipt>' );
		</script>
	</body>
</html>