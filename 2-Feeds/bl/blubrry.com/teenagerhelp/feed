jQuery(document).ready(function($) {
	if( jQuery('.nowplaying_share').position() == null )
		return;
		
	var share_position = jQuery('.nowplaying_share').position().top;
	var mycast_position = 0;
	if( jQuery('.nowplaying_mycast').length > 0 )
		mycast_position = jQuery('.nowplaying_mycast').position().top;
	var button_count = jQuery('.link_button').length;
	
	//alert(share_position + ' : ' + mycast_position);
	
	jQuery('.nowplaying_share').popup( { popup_item: '.nowplaying_share_popup', onclick: true, onhover: false, offsetRelative: true, offsetLeft: 40, offsetTop: (share_position+35), delay: 900, callback: function(data) {
			SetShareOptions( data.href, data.title );
		}
	});
	jQuery('.nowplaying_mycast').popup( { popup_item: '.nowplaying_mycast_popup', onclick: true, onhover: false, offsetRelative: true, offsetLeft: 40, offsetTop: (mycast_position+(button_count*20)+35 ), delay: 900, callback: function(data) {
			SetMyCastOptions( data.href );
		}
	});
	
});

</script>
