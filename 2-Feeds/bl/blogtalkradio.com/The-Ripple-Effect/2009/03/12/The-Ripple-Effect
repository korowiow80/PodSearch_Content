<script type="text/javascript" language="javascript" src="/JavaScript/player/jwplayer.js"></script>

<script type="text/javascript" language="javascript">
    EpPlayerHTML5 = {
        init: function (s) {
            args = s;
            playListUrl = args.playlistURL;
            EpPlayerHTML5.createPlaylistHTML5();
        },
        randomNumber: function () {
            var rnNow = new Date();
            var rn = rnNow.getTime().toString().split(".").join("");
            var rn = parseFloat(rn); //back to number
            return rn;
        },
        createPlaylistHTML5: function () {
            playlistHTML5 = [];
            if (!!document.createElement("video").canPlayType) { //supports HTML5				
                $.get(unescape(args.playlistURL), function (data) {
                    if ($(data).find("location").length > 0) {
                        //audio ads
                        if ($(data).find("audioad locations").length > 0) {
                            var _audioAds = $(data).find("audioad locations");
                            $(_audioAds).find("location").each(function (index) {
                                //var _mp3 = $(this).text().substring( 0,$(this).text().toLowerCase().indexOf(".mp3")+4 );
                                //var _qs = escape($(this).text().substring( $(this).text().toLowerCase().indexOf("?"),$(this).text().length ));
                                //var _adUrl = _mp3 + _qs;								
                                if ($(this).text().toLowerCase().indexOf("onetenthsecondsilence") < 0) {
                                    var _ad = "{file:'" + $(this).text() + "', image:'/img/ad_player_audio_400x276.gif', provider: 'sound', type: 'sound'}";
                                    playlistHTML5.push(_ad);

                                }
                            });
                        }
                        if ($(data).find("track:last location:first[play='false']").length > 0) {
                            //upcoming shows
                            playlistHTML5 = []; //wipe out ads
                            _audioShow = "{file:'', image:'" + args.image + "'}";
                        }
                        else {
                            _audioShow = "{file:'" + $(data).find("track:last location:first").text() + "', image:'" + args.image + "', provider: 'sound', type: 'sound'}";
                        }
                        playlistHTML5.push(_audioShow); //show comes last
                        EpPlayerHTML5.createPlayer();
                    }
                });
            }
            else { //no HTML5 support
                EpPlayerHTML5.createPlayer();
            }
        },
        createPlayer: function () {
            /* not moved:  mp1.addVariable("javascriptid", flashID); */
            var webRoot = window.location.protocol + "//" + window.location.host + "/";
            var _modes = [];
            var _html5 = "{type: 'html5', config:{playlist:[" + playlistHTML5 + "],repeat: 'list',icons: 'true',type: 'sound', height: 325, width: 400} }"
            var _flash = "{type: 'flash', src: '/swf/jwplayer.swf',	config:{playlistpath:'" + unescape(args.playlistURL + "&" + EpPlayerHTML5.randomNumber()) + "',icons: 'false', height: 325, width: 400} }";
            var _download = "{type: 'download'}"
            if (location.href.toLowerCase().indexOf("mode=html5") > -1) {
                _modes.push(_html5, _flash, _download);
            }
            else {
                _modes.push(_flash, _html5, _download);
            }
            _modes = eval("[" + _modes + "]");
            var flashvars = {
                flashplayer: "/swf/jwplayer.swf", //"/btrplayer300.swf",
                autostart: (args.autostart ? "true" : "false"),
                displayclick: "link",
                controlbar: "bottom",
                'controlbar.idlehide': "false",
                viral: { allowmenu: "false" },
                volume: "80",
                //icons: "false",
                localembed: (args.localembed ? args.localembed : "showpage"),
                adaptvjw5: {
                    key: "wwwblogtalkradiocom",
                    zid: "wwwblogtalkradiocom",
                    videoPlayerId: "Player1",
                    vid: "1234567" //????
                },
                plugins: "/swf/btrplugin.swf",
                skin: "/JavaScript/player/skin/stormtrooper.zip",
                callback: args.callback && typeof (args.callback) !== 'undefined' ? args.callback : '/FlashPlayerCallback.aspx?referrer_url=show',
                backcolor: "F6F6F6",
                screencolor: "F6F6F6",
                companionid: "companion1",
                duration: "0",
                bufferlength: "0",
                provider: "sound",
                type: "sound",
                colorbehindbar: "0xf6f6f6",
                events: {
                    onComplete: function () {
                        $(".ios-poster").remove();
                        //remove ad from playlist after episode completes
                        if ($("video").length && jwplayer().getPlaylistItem().index > 0) {
                            ///////////playlistHTML5.splice(0,1);
                            jwplayer().load("[" + playlistHTML5 + "]");
                            //jwplayer().stop();
                        }
                    },
                    onError: function () {
                        jwplayer().playlistNext();
                        $(".ios-poster").remove();
                    },
                    onReady: function () {
                        //if( $("video").length > 0 ) {alert('x');
                        $("video").attr("type", "audio/mpeg");
                        $(".ios-poster").remove();
                        $("#single1").addClass("ios-wrapper");
                        //alert(playlistHTML5);

                        //}
                    },

                    onPlay: function () {
                        if ($("video").length) {
                            //$("video").show("5000"); //needed so ad url has time to redirect
                            $("video").attr("type", "audio/mpeg");
                            $("video").attr("controls", "true");
                            $(".ios-poster").remove();
                            var _img = $("<img/>");
                            $(_img).addClass("ios-poster");
                            //$("#single1_video_wrapper").css("background","url(" + jwplayer().getPlaylistItem().image + ")");
                            //$("video").css("-webkit-mask-box-image","url(" + jwplayer().getPlaylistItem().image + ")");
                            $(_img).attr("src", jwplayer().getPlaylistItem().image);
                            //$(_img).appendTo("#single1_displayarea");
                            $(_img).appendTo("#single1_video_wrapper");
                            $("#single1_displayarea").addClass("ios-displayarea");
                        }
                    }
                },

                modes: _modes

            };
            if (args.link && typeof (args.link) !== 'undefined') { flashvars.link = args.link; };
            if (args.image && typeof (args.image) !== 'undefined') { flashvars.image = args.image; };
            if (args.title && typeof (args.title) !== 'undefined') { flashvars.adaptvjw5.title = args.title; };
            if (args.desc && typeof (args.desc) !== 'undefined') { flashvars.adaptvjw5.description = args.desc; };

            if (document.getElementById('single1') != null) { EpPlayerHTML5.destroyPlayer(); };

            $("#divPlayer").attr("id", "single1").attr("name", "single1");
            jwplayer("single1").setup(flashvars);
        },
        destroyPlayer: function () {
            var pp = document.getElementById("single1");
            if (pp) {
                var pr = pp.parentNode;
                pr.removeChild(pp);
            };
            $('#single1').remove();
            if (!$('#divPlayer').exists()) { $('.player-movie').prepend("<div id='divPlayer'></div>"); };
            EpPlayerHTML5.createPlayer();
        }

    //return { init: init, createPlayer: createPlayer, destroyPlayer: destroyPlayer, randomNumber: randomNumber, createPlaylistHTML5: createPlaylistHTML5 };
    };

</script>

