    <script type="text/javascript" language="javascript">
        (function () {
            var addScriptToHead = function (url, async, onload) {
                var head = document.getElementsByTagName("head")[0] || document.documentElement;
                var s = document.createElement('script');
                s.type = 'text/javascript';
                if (async) {
                    s.async = true;
                }
                if (typeof onload == 'function') {
                    // most browsers
                    s.onload = onload;
                    // IE 6, 7
                    s.onreadystatechange = function() {
                        if (this.readyState == 'complete') {
                            onload();
                        }
                    };
                }
                s.src = url;
                head.appendChild(s);
                return s;
            };

            var asyncWidgetsLoad = function () {
                addScriptToHead('http://platform.twitter.com/widgets.js', true);
                addScriptToHead('http://s7.addthis.com/js/250/addthis_widget.js#pubid=blogtalkradio&domready=1', true, function() {
                    // AddThis functionality
                    function addthisReady(evt) {
                        addthis.addEventListener('addthis.menu.share', MYBTR.UList.shareComplete);
                    }
                    addthis.addEventListener('addthis.ready', addthisReady);
                });
            };

            if (window.attachEvent) {
                window.attachEvent('onload', asyncWidgetsLoad);
            } else {
                window.addEventListener('load', asyncWidgetsLoad, false);
            }
        })();

</script>

    

