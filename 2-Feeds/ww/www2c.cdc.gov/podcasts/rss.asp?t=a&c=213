<!--//
	function showItems(id) {
		document.getElementById('inner_'+id).style.display="inline";
		document.getElementById('show_'+id).style.display="none";
		document.getElementById('hide_'+id).style.display="inline";
		document.getElementById('container_'+id).style.height="300px";
	}
	
	function hideItems(id) {
		document.getElementById('inner_'+id).style.display="none";
		document.getElementById('hide_'+id).style.display="none";
		document.getElementById('show_'+id).style.display="inline";
		document.getElementById('container_'+id).style.height="0px";
	}
//-->
</script>
