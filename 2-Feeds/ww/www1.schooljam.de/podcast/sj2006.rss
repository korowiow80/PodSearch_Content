<script language="javascript" type="text/javascript">
var compulsoryFieldsInSJ=new Array();
compulsoryFieldsInSJ[0]="stdIn3";
function checkFieldsInSJ ()
{
    var checkOK=true;
    for (var i=0; i<compulsoryFieldsInSJ.length; i++)
    {
        var cfObj=document.getElementById(compulsoryFieldsInSJ[i]);
        if (cfObj!=null)
        {
            if (cfObj.type.toLowerCase()=="text")
            {
                if (cfObj.value.match(/^\s*$/)) checkOK=false;
            }
            else if (cfObj.type.toLowerCase()=="radio" || cfObj.type.toLowerCase()=="checkbox")
            {
				var tmpObj=document.getElementsByName(cfObj.name);
                var tmpCheck=false;
                for (var j=0; j<tmpObj.length; j++)
                {
                    if (tmpObj[j].checked==true)
                    {
                        tmpCheck=true;
                        break;
                    }
                }
                checkOK=tmpCheck;
            }
            else if (cfObj.type.toLowerCase().indexOf("select")>=0)
            {
                if (compulsoryFieldsInSJ[i]=="stdIn12" && cfObj.selectedIndex==0) checkOK=false;
            }
        }
        if (!checkOK) break;
    }
    if (checkOK) document.getElementById('subscribe_sj').submit();
    else window.alert("Bitte füllen Sie alle mit einem * markierten Felder aus.");
}

</script>

        
