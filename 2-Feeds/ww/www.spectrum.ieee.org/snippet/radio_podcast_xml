<script>
function showMenu(channel, id){
	//console.log('showing: ' + id + ' for ' + this);
	//console.log($('#' + id));
	var menu = $('#' + id)
	menu.show();

	var offset = $(channel).offset();
	//console.log(offset)
	menu.css('top', offset.top + 27);
	menu.css('left', offset.left);


	var channelId = $(channel).attr('id');
	//console.log(channelId);
	//console.log("nav-" + channelId + "-off" + "/" + "nav-" + channelId + "-hover")
	$(channel).removeClass(channelId + "-off").addClass(channelId + "-hover");
}

function hideMenu(channel, id){
	//TODO use mouseleave as an event handler
	//console.log('hiding: ' + '#' + id);
	var channelId = $(channel).attr('id');
	$(channel).removeClass(channelId + "-hover").addClass(channelId + "-off");
	
	$('#' + id).hide();
}
</script>


