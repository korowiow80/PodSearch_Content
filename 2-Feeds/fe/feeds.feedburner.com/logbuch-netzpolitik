		<title>LNP024 Episches Mäandern</title>
		<link>http://feedproxy.google.com/~r/logbuch-netzpolitik/~3/6gRjcILgMzE/</link>
		<comments>http://logbuch-netzpolitik.de/lnp024-episches-maeandern/#comments</comments>
		<pubDate>Fri, 18 May 2012 12:32:17 +0000</pubDate>
		<dc:creator>tim@metaebene.me</dc:creator>
				<category><![CDATA[Podcast]]></category>
		<category><![CDATA[Anonymous]]></category>
		<category><![CDATA[Ansgar Heveling]]></category>
		<category><![CDATA[bka]]></category>
		<category><![CDATA[bundestrojaner]]></category>
		<category><![CDATA[flattr]]></category>
		<category><![CDATA[Google]]></category>
		<category><![CDATA[ios app store]]></category>
		<category><![CDATA[meinungsfreiheit]]></category>
