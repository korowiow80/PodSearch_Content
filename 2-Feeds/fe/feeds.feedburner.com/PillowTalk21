// Send Google Events on Play
ps.message.add({'play': function(data) {

	var media = data.media;
	if (media) {
		ps.google.track_event('Instrumentation', 'Play Start by Show', 'sId=' + media.parent_id, 1);
		ps.google.track_event('Instrumentation', 'Play Start by Episode', 'eId=' + media.key_id, 1);
	}					 
}});

// Send Google Event 'Play Time Viewed 30 sec'
(function(){
	var original_show_id = -1; // Initialize to impossible value
	var play_time = 30;
	var timer;

	var sendGoogleEngaged = function(){
			ps.google.track_event('Instrumentation', 'Play Time Viewed 30 sec', 'sId=' + original_show_id, play_time);
			play_time += 30;			
	}

	ps.message.add({'play': function(data) {
		var media = data.media;
			
		// We are on new show so reset Timer
		if(original_show_id != media.parent_id)
		{
			if(typeof(timer) != 'undefined')
			{
				clearInterval(timer);
			}
			play_time = 30;
			timer = setInterval(sendGoogleEngaged,30000);
			original_show_id = media.parent_id;
		}

	}});
}
)();

if ( typeof(__iframe_detected) == 'undefined' || !__iframe_detected ) {
	ps.player_with_que(args);
}

});


</script>

