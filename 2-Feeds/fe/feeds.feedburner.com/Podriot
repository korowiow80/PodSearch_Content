        <script type="text/javascript" language="javascript">
            var popularCategories3 = '';
            
            if (popSearches && popSearches.length > 0) {
	            var google_categories = popSearches;
            	
	            if (google_categories.length > 0) {
		            popularCategories3 += '<table border="0" cellspacing="0" cellpadding="0" width="100%">';
		            var columnCount = parseInt(3);
            			
		            for (var i = 0; i < google_categories.length; i++) {
			            if (i % columnCount == 0)
			            {
				            popularCategories3 += '<tr>';
			            }			
            		
			            popularCategories3 += '<td>';
            			
			            popularCategories3 += '<a id="ps' + i.toString() + '" href=""><b>' + google_categories[i].term + '</b></a>';
			            popularCategories3 += '<script type="text/javascript" language="javascript">';
			            popularCategories3 += 'var ps' + i.toString() + ' = document.getElementById(\'ps' + i.toString() + '\'); if(ps' + i.toString() + ') {ps' + i.toString() + '.href = \'/?q=' + encodeURIComponent(google_categories[i].term) + '&kot=' + google_categories[i].token + '&srcht=p\';}';
			            popularCategories3 += '<\/script><br />';
            			
			            if (google_categories[i].subcategories && google_categories[i].subcategories.length > 0)
			            {
				            var catId = '00' + i.toString();
            				
				            for (var j = 0; j < google_categories[i].subcategories.length; j++)
				            {
					            var subCatId = '00' + j.toString();
            					
					            popularCategories3 += '<a  id="ps' + catId.substring(catId.length - 2) + subCatId.substring(subCatId.length - 2) + '" href="">' + google_categories[i].subcategories[j].term + '</a>';
					            popularCategories3 += '<script type="text/javascript" language="javascript">';
					            popularCategories3 += 'var ps' + catId.substring(catId.length - 2) + subCatId.substring(subCatId.length - 2) + ' = document.getElementById(\'ps' + catId.substring(catId.length - 2) + subCatId.substring(subCatId.length - 2) + '\'); if(ps' + catId.substring(catId.length - 2) + subCatId.substring(subCatId.length - 2) + ') {ps' + catId.substring(catId.length - 2) + subCatId.substring(subCatId.length - 2) + '.href = \'/?q=' + encodeURIComponent(google_categories[i].subcategories[j].term) + '&kot=' + google_categories[i].subcategories[j].token + '&srcht=p\';}';
					            popularCategories3 += '<\/script><br />';
				            }
			            }
            			
			            popularCategories3 += '<br /></td>';
            			
			            if (i % columnCount == (columnCount - 1)) {
				            popularCategories3 += '</tr>';
			            }
		            }
            		
		            popularCategories3 += '</table>';
	            }
            }
            
            document.write(popularCategories3);
        </script>

