        var onclickScript = '';
       onclickScript = 'onclick="processClick(\'' + 'Wzp9ZGNjAFMxCFMwCGNznG0zMG0zow0zL3MkCF0kZQp5ZmHjZwR5ZGVmAwV0AQD4WzIaCGVjZGVjAwN2ZGtkZQZ5WzA5CGRzrQ0zqTp9ZwWsZ2A1WzMapQ0jWaO1CGRkZQR3BFMhrw0jWzMjCGNznT5aCGRzpUN9FRLzqTL9AvMjpm0kZQp5ZmHjZwR5ZGVmAwV0AQD4-1\');"';
        for (var i = 0; i < google_ads.length; i++) {
	       
            
            
	        
	       ads += '<a target="_blank" href="' + google_ads[i].url + '" id="gad' + i.toString() + '_1" style="text-decoration:underline;color:#00F;cursor:pointer;"  title="' + google_ads[i].visible_url + '" ' + onclickScript + '><span class="linkhd">'+ google_ads[i].line1 +'</span></a>';
	        
