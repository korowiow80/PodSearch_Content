    <script type="text/javascript" language="javascript">
    
    if (google_ads && google_ads.length > 0)
    {
        if (document.getElementById('pagecontent') != null) {
            document.getElementById('pagecontent').style.display = '';
        }
        
        if (document.getElementById('pagecontent2') != null) {
            document.getElementById('pagecontent2').style.display = 'none';
        }
        
        if (document.getElementById('pagecontent3') != null) {
            document.getElementById('pagecontent3').style.display = 'none';
        }
    }
    else if (popSearches && popSearches.length > 0)
    {
        if (document.getElementById('pagecontent') != null) {
            document.getElementById('pagecontent').style.display = 'none';
        }
        
        if (document.getElementById('pagecontent2') != null) {
            document.getElementById('pagecontent2').style.display = '';
        }
        
        if (document.getElementById('pagecontent3') != null) {
            document.getElementById('pagecontent3').style.display = 'none';
        }
    }
    else
    {
        if (document.getElementById('pagecontent') != null) {
            document.getElementById('pagecontent').style.display = 'none';
        }
        
        if (document.getElementById('pagecontent2') != null) {
            document.getElementById('pagecontent2').style.display = 'none';
        }
        
        if (document.getElementById('pagecontent3') != null) {
            document.getElementById('pagecontent3').style.display = '';
        }
    }
    
    if (adult == 'true')
    {
        if (document.getElementById('relatedsearches') != null) {
            document.getElementById('relatedsearches').style.borderTop = '#000000 1px solid';
        }
    }
    
    </script>
