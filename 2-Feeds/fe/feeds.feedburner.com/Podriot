    <script type="text/javascript" language="javascript">
        relatedSearches = '';
    
        if (linkUnits && linkUnits.length > 0) {
	        var google_link_units = linkUnits;
	        
	        if (google_link_units.length > 0) {
	            relatedSearches += '<div id="relatedsearches">';

	            if (feedbackUrl > '') {
	                relatedSearches += '<div><span class="related"><a style="text-decoration:none;color:#000077;" TARGET="_blank" href="' + feedbackUrl + '" >Related Searches</a></span></div><br class="clear"/>';
	            }
	            else {
	                relatedSearches += '<div><span class="related">Related Searches</span></div><br class="clear"/>';
	            }
	            
	            for (var i=0; i<google_link_units.length; i++) {
	                relatedSearches += '<div><table border="0" cellpadding="0" cellspacing="0"><tr><td valign="top" style="padding-top:2px;"><img src="http://ak.imgaft.com/images/cp/icn_arrow.gif" width="9" height="10" alt=""/></td><td>&nbsp;</td><td><a id="rs' + i.toString() + '" href="" title="Search For:' + google_link_units[i].term + '">' + google_link_units[i].term + '</a></td></tr></table></div><br class="clear"/>';
	                relatedSearches += '<script type="text/javascript" language="javascript">';
	                relatedSearches += 'var rs' + i.toString() + ' = document.getElementById(\'rs' + i.toString() + '\');';
	                relatedSearches += 'if(rs' + i.toString() + ') {rs' + i.toString() + '.href = \'/?q=' + encodeURIComponent(google_link_units[i].term) + '&srcht=r&kot=' + google_link_units[i].token + '\';}';
	                relatedSearches += '<\/script>';
                }
	            
	            relatedSearches += '</div>';
	        }
	    }
        
        document.write(relatedSearches);
    </script>

            
