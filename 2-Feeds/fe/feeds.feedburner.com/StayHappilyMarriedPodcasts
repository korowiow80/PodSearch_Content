		<category><![CDATA[advice]]></category>
		<category><![CDATA[anxiety]]></category>
		<category><![CDATA[appreciation]]></category>
		<category><![CDATA[books]]></category>
		<category><![CDATA[coaching]]></category>
		<category><![CDATA[communication]]></category>
