    <!-- Start SproutCore on Page Load -->
    <script type="text/javascript">

		if ( bootstrap.Browser.IE) {(function(){
				try {
					document.documentElement.doScroll("left");
				} catch( error ) {
					setTimeout( arguments.callee, 0 );
					return;
				}
				SC.didLoad();
			})();
		}else if ( bootstrap.Browser.Opera ){
				document.addEventListener( "DOMContentLoaded", function () {
					for (var i = 0; i < document.styleSheets.length; i++)
						if (document.styleSheets[i].disabled) {
							setTimeout( arguments.callee, 0 );
							return;
						}
					SC.didLoad();
				}, false);
		}else if( bootstrap.Browser.Firefox ){
					if (document.addEventListener) {
						document.addEventListener("DOMContentLoaded", SC.didLoad, false);
					}
			}else{
				window.onload=SC.didLoad;
			}
			
			</script>
	
