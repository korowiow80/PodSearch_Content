    <script type="text/javascript">
    appConfig = bootstrap.env['mail'];

    
    var extraParams = '';
    if(window.location.hash)
	{
		extraParams = '&anchor='+window.location.hash.substring(1);
	}
    if(window.location.search.indexOf("flow=upgrade") != -1)
	{
		extraParams = extraParams + '&flow=upgrade';
		var akeyIndex = window.location.search.indexOf("&aKey=");
		if(akeyIndex != -1)
		{
			extraParams = extraParams+'&aKey='+window.location.search.substring(akeyIndex+6,akeyIndex+22);
		}
	}
      window.stats={};var transport=null;var isc=(bootstrap.readCookie('isc-www.me.com')||bootstrap.readCookie('isc-secure.me.com')||bootstrap.readCookie('isc-apple.com')||'');function startUp(url,param){window.appReqTime=new Date().getTime();window.stats[url]={start:window.appReqTime};if(window.XMLHttpRequest){transport=new XMLHttpRequest();}else if(window.ActiveXObject){try{transport=new ActiveXObject("Msxml2.XMLHTTP");}catch(e){try{transport=new ActiveXObject("Microsoft.XMLHTTP");}catch(e){}}}if(!transport){return false;}transport.onreadystatechange=function(){if(transport.readyState==4){readServerResponse(transport);}};fullUrl=url+param;transport.open('GET',fullUrl,true);transport.setRequestHeader('X-Mobileme-Version','1.0');if(isc!=''){transport.setRequestHeader('X-Mobileme-Isc',isc);};transport.send(null);};var lang=appConfig.lang;url=appConfig.rootURL;var urlLang=window.location.pathname.toString().match(/\/.+\/(..)\/?$/);if(urlLang&&urlLang[1])lang=urlLang[1].toLowerCase();startUp(url,'?lang='+lang+extraParams);function readServerResponse(originalResponse){window.serverReqRecdTime=new Date().getTime();window.stats[url].end=window.serverReqRecdTime;window.serverResponse=originalResponse;if(typeof(Mac)!="undefined"&&Mac.dockController&&SC.page){bootstrap.ignoreAicCookie = false;Mac.dockController.handleAicCookie();}}
      </script>


		

	
