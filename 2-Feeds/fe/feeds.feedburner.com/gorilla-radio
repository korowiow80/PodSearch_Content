    <pubDate>Tue, 11 Nov 2008 05:40:00 +0100</pubDate>
    <title>Gorilla Radio with Chris Cook, Steve Filipovic, David Swanson, J9 Nov. 10, 2008</title>
    <link>http://feedproxy.google.com/~r/gorilla-radio/~3/HFrAtc6ROC8/index.php</link>
    <guid isPermaLink="false">http://www.gorilla-radio.com/index.php?id=262</guid>
    <dc:creator>editor@atlanticfreepress.com (Atlantic Free Press - Chris Cook)</dc:creator>
    <itunes:author>marah</itunes:author>
    <itunes:explicit>no</itunes:explicit>
    <comments>http://www.gorilla-radio.com/index.php?id=262#comments</comments>
    <itunes:keywords />
    <category>filipovic</category>
    <category />
    <category>swanson</category>
    <category />
