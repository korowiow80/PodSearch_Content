    <title>Senior Centre in Havana</title>
    <link>http://feedproxy.google.com/~r/gorilla-radio/~3/B3s_vD4D3ZE/index.php</link>
    <guid isPermaLink="false">http://www.gorilla-radio.com/index.php?id=238</guid>
