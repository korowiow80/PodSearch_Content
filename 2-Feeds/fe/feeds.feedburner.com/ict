<script type="text/javascript">
function changeThisSearchParams()
{
	document.getElementById('hidden_ext').value = '1';
		
	if(document.getElementById("macinasearch_button1") && document.getElementById("macinasearch_button1").checked)
	{
		document.getElementById('_catFilter_types').value = document.getElementById('msb_cat_filter').value;
		document.getElementById('_geo').value = document.getElementById('msb_geo_filter').value;
		document.getElementById('tx_msb_tag_filter').value = document.getElementById('msb_tag_filter').value;
		document.getElementById('tx_msb_domainsrc_filter').value = document.getElementById('msb_domainsrc_filter').value;
		document.getElementById('tx_msb_type_filter').value = document.getElementById('msb_type_filter').value;
	}

	document.getElementById('searchform').submit();
}
function goAdvancedSearchPage()
{
	var link;
	if(document.getElementById("macinasearch_button1").checked)
		link = document.getElementById("advansed_link").value
	else link = document.getElementById("all_link").value

	location.href=link
}
</script>

