<script type="text/javascript" src="http://ict.zunia.org/typo3/ext/dg_content/pi1/res/js/taxonomy_popup.js"></script><script type="text/javascript" src="/fileadmin/treeMenu/dtree.js"></script><script type="text/javascript">
var taxCategoryTreeLoaded = false;
var taxCategoryTree = null;
var taxCatSearchTimeoutHandler = null;

var taxGeographyTreeLoaded = false;
var taxGeographyTree = null;
var taxGeoSearchTimeoutHandler = null;

var taxTagSearchTimeoutHandler = null;


$(document).ready(
	function(){
		var taxonomy_link = $('.taxonomy_popup');
	
		//if(taxonomy_link && (taxonomy_link.length > 0)){
			var taxonomy_popup = $("#dgcontent_taxonomy_popup");
			$("#dgcontent_taxonomy_popup").css('display', 'block');
			
			if(taxonomy_popup && (taxonomy_popup.length > 0)){
				taxonomy_popup.dialog({
					autoOpen: false,
					modal: true,
					width: 700,
					height: 600,
					title: 'Create a new filter',
					buttons:{
						"Cancel": function(){$(this).dialog("close");},
						"Add": function(){taxSubmit($(this));}
					},
                    open:function(){taxOnDialogOpen();},
					close:function(){}
				});
				
                taxonomy_popup.bind('scroll',function(){
					taxHide('dgcontent_taxonomy_cat_sug');
                    taxHide('dgcontent_taxonomy_geo_sug');
				});
                
				taxonomy_link.live('click', function(){
					taxonomy_popup.dialog("open");
					return false;
				});
			}
		//}
	}
);

function taxOnDialogOpen(){
	taxCleanControls();
	taxHideMessages();
    
    
    if(!taxCategoryTreeLoaded){
    	taxLoadCategoryTree();
    }
	
    
    
    if(!taxGeographyTree){
    	taxLoadGeographyTree();
    }
	
}


function taxLoadCategoryTree(){
	taxSetDivContentToLoader('dgcontent_taxonomy_category_sel');
    $.ajax({
		type: 'POST',
		url: 'index.php?eID=dg_content_ajax&lang_uid=0&lang=',
		data: {fnc_name:'taxonomy',mode:'catTree',pidList:'11464'},
		success: function(data){
					taxProcessCatTree(data);
				},
		error: function(request,error){
					taxSetDivContentToError('dgcontent_taxonomy_category_sel', error);
				},
		dataType: "json"
	});
}

function taxProcessCatTree(data){
	if(data.result == 'error'){
    	taxSetDivContentToError('dgcontent_taxonomy_category_sel', '');
    }
    else if(data.result == 'ok'){
    	taxCategoryTreeLoaded = true;
        taxBuildCategoryTree(data.nodes);
        document.getElementById('dgcontent_taxonomy_category_sel').innerHTML = taxCategoryTree;
    }
    else{
    	$("#dgcontent_taxonomy_category_sel").html('');
    }
}

function taxBuildCategoryTree(nodes){
	taxCategoryTree = new dTree('taxCategoryTree');
	taxCategoryTree.icon = {
		root : '/fileadmin/treeMenu/img/base.gif',
		folder : '/fileadmin/treeMenu/img/folder.gif',
		folderOpen : '/fileadmin/treeMenu/img/folderopen.gif',
		node : '/fileadmin/treeMenu/img/page.gif',
		empty : '/fileadmin/treeMenu/img/empty.gif',
		line : '/fileadmin/treeMenu/img/line.gif',
		join : '/fileadmin/treeMenu/img/join.gif',
		joinBottom : '/fileadmin/treeMenu/img/joinbottom.gif',
		plus : '/fileadmin/treeMenu/img/plus.gif',
		plusBottom : '/fileadmin/treeMenu/img/plusbottom.gif',
		minus : '/fileadmin/treeMenu/img/minus.gif',
		minusBottom	: '/fileadmin/treeMenu/img/minusbottom.gif',
		nlPlus : '/fileadmin/treeMenu/img/nolines_plus.gif',
		nlMinus : '/fileadmin/treeMenu/img/nolines_minus.gif'
	};
	
	taxCategoryTree.config.useIcons = false;
	taxCategoryTree.config.useLines = true;

	taxCategoryTree.add(0,-1,'');
	
	var nodeCount = nodes.length;
	for(var i = 0; i < nodeCount; i++){
		if(nodes[i].selectable){
			taxCategoryTree.add(nodes[i].uid, nodes[i].pid, '<span class="contrib-tree-select">'+nodes[i].name+'</span>',"javascript:taxAddOption('dgcontent_taxonomy_selected_categories' ,'"+nodes[i].clean_name+"', '"+nodes[i].uid+"');");
		}
		else{
			taxCategoryTree.add(nodes[i].uid, nodes[i].pid, '<span class="contrib-tree-nonselect">'+nodes[i].name+'</span>', '');
		}
	}
}



function taxLoadGeographyTree(){
	taxSetDivContentToLoader('dgcontent_taxonomy_geography_sel');
    $.ajax({
		type: 'POST',
		url: 'index.php?eID=dg_content_ajax&lang_uid=0&lang=',
		data: {fnc_name:'taxonomy',mode:'geoTree'},
		success: function(data){
					taxProcessGeoTree(data);
				},
		error: function(request,error){
					taxSetDivContentToError('dgcontent_taxonomy_geography_sel', error);
				},
		dataType: "json"
	});
}

function taxProcessGeoTree(data){
	if(data.result == 'error'){
    	taxSetDivContentToError('dgcontent_taxonomy_geography_sel', '');
    }
    else if(data.result == 'ok'){
    	taxGeographyTreeLoaded = true;
        taxBuildGeographyTree(data.nodes);
        document.getElementById('dgcontent_taxonomy_geography_sel').innerHTML = taxGeographyTree;
    }
    else{
    	$("#dgcontent_taxonomy_geography_sel").html('');
    }
}

function taxBuildGeographyTree(nodes){
	taxGeographyTree = new dTree('taxGeographyTree');
	taxGeographyTree.icon = {
		root : '/fileadmin/treeMenu/img/base.gif',
		folder : '/fileadmin/treeMenu/img/folder.gif',
		folderOpen : '/fileadmin/treeMenu/img/folderopen.gif',
		node : '/fileadmin/treeMenu/img/page.gif',
		empty : '/fileadmin/treeMenu/img/empty.gif',
		line : '/fileadmin/treeMenu/img/line.gif',
		join : '/fileadmin/treeMenu/img/join.gif',
		joinBottom : '/fileadmin/treeMenu/img/joinbottom.gif',
		plus : '/fileadmin/treeMenu/img/plus.gif',
		plusBottom : '/fileadmin/treeMenu/img/plusbottom.gif',
		minus : '/fileadmin/treeMenu/img/minus.gif',
		minusBottom	: '/fileadmin/treeMenu/img/minusbottom.gif',
		nlPlus : '/fileadmin/treeMenu/img/nolines_plus.gif',
		nlMinus : '/fileadmin/treeMenu/img/nolines_minus.gif'
	};
	
	taxGeographyTree.config.useIcons = false;
	taxGeographyTree.config.useLines = true;

	taxGeographyTree.add(0,-1,'');
    taxGeographyTree.add(999999,0,'<span class="contrib-tree-select">Worldwide</span>',"javascript:taxAddOption('dgcontent_taxonomy_selected_geo' ,'Worldwide', '999999');");
	
	var nodeCount = nodes.length;
	for(var i = 0; i < nodeCount; i++){
		if(nodes[i].selectable){
			taxGeographyTree.add(nodes[i].uid, nodes[i].pid, '<span class="contrib-tree-select">'+nodes[i].name+'</span>',"javascript:taxAddOption('dgcontent_taxonomy_selected_geo' ,'"+nodes[i].clean_name+"', '"+nodes[i].uid+"');");
		}
		else{
			taxGeographyTree.add(nodes[i].uid, nodes[i].pid, '<span class="contrib-tree-nonselect">'+nodes[i].name+'</span>', '');
		}
	}
}


function taxSubmit(taxDialogDiv){
	var error = taxCheckError();
    
    if(error){
    	$("#dgcontent_taxonomy_error").html(error);
		$("#dgcontent_taxonomy_error").show('fast');
		$("#dgcontent_taxonomy_popup").animate({scrollTop: 0},'fast');
    }
    else{
    	taxHideMessages();
        taxSetDivContentToLoader('dgcontent_taxonomy_placeholder');
        
        var catArray;
        var geoArray;
        var tagArray;
        var keywords;
        
        
        catArray = taxGetOptionListValues('dgcontent_taxonomy_selected_categories');
        
        
        
        geoArray = taxGetOptionListValues('dgcontent_taxonomy_selected_geo');
        
        
        
        tagArray = taxGetOptionListValues('dgcontent_taxonomy_selected_tag');
        
        
        
        keywords = '';
        
       
        if((catArray.length > 0) || (geoArray.length > 0) || (tagArray.length > 0) || keywords){
        	$("#dgcontent_taxonomy_popup").animate({scrollTop: 0},'fast');
        	var taxData = {'categories': catArray, 'geography': geoArray, 'tags':tagArray, 'keywords':keywords};
        	submitCreateFilter(taxData, taxDialogDiv);
        }
        else{
        	$("#dgcontent_taxonomy_placeholder").html("");
        	$("#dgcontent_taxonomy_error").html("Empty selection");
			$("#dgcontent_taxonomy_error").show('fast');
			$("#dgcontent_taxonomy_popup").animate({scrollTop: 0},'fast');
        }
    }
}

function taxCheckError(){
	var error = "";
    var optionList;
    var catLength = 0;
    var tagLength = 0;
    
    
    optionList = document.getElementById('dgcontent_taxonomy_selected_categories');
    catLength = optionList.length;
    
	
    
    
    if(catLength > 1){
		error = error + "Maximum allowed categories:1" + "<br />";	
    }
    

    
    
    
    optionList = document.getElementById('dgcontent_taxonomy_selected_geo');
    var geoLength = optionList.length;
    
	
    
    
    if(geoLength > 1){
		error = error + "Maximum allowed regions/countries:1" + "<br />";	
    }
    

    
    
    
    optionList = document.getElementById('dgcontent_taxonomy_selected_tag');
    tagLength = optionList.length;
    
	
    
    
    if(tagLength > 1){
		error = error + "Maximum allowed tags:1" + "<br />";	
    }
    

    
    
    
    if((catLength > 0) && (tagLength > 0)){
    	error = error + "Category and tag cannot be selected together" + "<br />";	
    }
    
    
    
    
    return error;
}

function taxCleanControls(){
    
    taxClearOptionList('dgcontent_taxonomy_selected_categories', 'all');
    
	$("#dgcontent_taxonomy_cat_sug").css('display', 'none');
	$("#dgcontent_taxonomy_cat_search").css('color', '#CCCCCC');
	$("#dgcontent_taxonomy_cat_search").attr('value', 'Search');
    
    
    
    taxClearOptionList('dgcontent_taxonomy_selected_geo', 'all');
    
	$("#dgcontent_taxonomy_geo_sug").css('display', 'none');
	$("#dgcontent_taxonomy_geo_search").css('color', '#CCCCCC');
	$("#dgcontent_taxonomy_geo_search").attr('value', 'Search');
    
    
    
    taxClearOptionList('dgcontent_taxonomy_selected_tag', 'all');
    
	$("#dgcontent_taxonomy_tag_sel").html('');
	$("#dgcontent_taxonomy_tag_search").css('color', '#CCCCCC');
	$("#dgcontent_taxonomy_tag_search").attr('value', 'Search');
    
    
    
}

function taxHideMessages(){
	$("#dgcontent_taxonomy_error").html('');
	$("#dgcontent_taxonomy_error").css('display', 'none');
		
	$("#dgcontent_taxonomy_success").html('');
	$("#dgcontent_taxonomy_success").css('display', 'none');
		
	$("#dgcontent_taxonomy_placeholder").html('');
}

function taxCatSearch(val){
	if(val.length < 3) return;
	
	if(taxCatSearchTimeoutHandler){
		clearTimeout(taxCatSearchTimeoutHandler);
	}

	taxCatSearchTimeoutHandler = setTimeout(function(){taxCatSearchSubmit(val)},500);
}

function taxCatSearchSubmit(val){
	if(val != document.getElementById('dgcontent_taxonomy_cat_search').value){
		return;
	}
	
    taxSetDivContentToLoader('dgcontent_taxonomy_cat_sug_inner');
	$("#dgcontent_taxonomy_cat_sug").show('fast');
	
    $.ajax({
		type: 'POST',
		url: 'index.php?eID=dg_content_ajax&lang_uid=0&lang=',
		data: {fnc_name:'taxonomy',mode:'catSearch',cat_search:val,pidList:'11464'},
		success: function(data){
					if(data.result == 'error'){
                        taxSetDivContentToError('dgcontent_taxonomy_cat_sug_inner', '');
                    }
                    else if(data.result == 'ok'){
                        document.getElementById('dgcontent_taxonomy_cat_sug_inner').innerHTML = data.out;
                    }
                    else{
                        $("#dgcontent_taxonomy_cat_sug_inner").html('');
                    }
				},
		error: function(request,error){
					taxSetDivContentToError('dgcontent_taxonomy_cat_sug_inner', error);
				},
        dataType: "json"
	});
}

function taxGeoSearch(val){
	if(val.length < 3) return;
	
	if(taxGeoSearchTimeoutHandler){
		clearTimeout(taxGeoSearchTimeoutHandler);
	}

	taxGeoSearchTimeoutHandler = setTimeout(function(){taxGeoSearchSubmit(val)},500);
}

function taxGeoSearchSubmit(val){
	if(val != document.getElementById('dgcontent_taxonomy_geo_search').value){
		return;
	}
	
    taxSetDivContentToLoader('dgcontent_taxonomy_geo_sug_inner');
	$("#dgcontent_taxonomy_geo_sug").show('fast');
	
    $.ajax({
		type: 'POST',
		url: 'index.php?eID=dg_content_ajax&lang_uid=0&lang=',
		data: {fnc_name:'taxonomy',mode:'geoSearch',geo_search:val},
		success: function(data){
					if(data.result == 'error'){
                        taxSetDivContentToError('dgcontent_taxonomy_geo_sug_inner', '');
                    }
                    else if(data.result == 'ok'){
                        document.getElementById('dgcontent_taxonomy_geo_sug_inner').innerHTML = data.out;
                    }
                    else{
                        $("#dgcontent_taxonomy_geo_sug_inner").html('');
                    }
				},
		error: function(request,error){
					taxSetDivContentToError('dgcontent_taxonomy_geo_sug_inner', error);
				},
        dataType: "json"
	});
}

function taxTagSearch(val){
	if(val.length < 3) return;
	
	if(taxTagSearchTimeoutHandler){
		clearTimeout(taxTagSearchTimeoutHandler);
	}

	taxTagSearchTimeoutHandler = setTimeout(function(){taxTagSearchSubmit(val)},500);
}

function taxTagSearchSubmit(val){
	if(val != document.getElementById('dgcontent_taxonomy_tag_search').value){
		return;
	}
	
    taxSetDivContentToLoader('dgcontent_taxonomy_tag_sel');
	
    $.ajax({
		type: 'POST',
		url: 'index.php?eID=dg_content_ajax&lang_uid=0&lang=',
		data: {fnc_name:'taxonomy',mode:'tagSearch',tag_search:val},
		success: function(data){
					if(data.result == 'err'){
                         $("#dgcontent_taxonomy_tag_sel").html(data.message);
                    }
                    else if(data.result == 'ok'){
                        document.getElementById('dgcontent_taxonomy_tag_sel').innerHTML = data.out;
                    }
                    else{
                        $("#dgcontent_taxonomy_tag_sel").html('');
                    }
				},
		error: function(request,error){
					taxSetDivContentToError('dgcontent_taxonomy_tag_sel', error);
				},
        dataType: "json"
	});
}

</script>
<script type="text/javascript">

function submitCreateFilter(taxData, taxDialogDiv){
	var categoryID = 0;
	var geographyID = 0;
	var tagsID = '';
	var redirectLink = 'http://zunia.org/my-zunia/alertsfilters/?pageID=11680&save=1';
	if(taxData.categories.length > 0) 
		categoryID = taxData.categories[0];
	if(taxData.geography.length > 0) 
		geographyID = taxData.geography[0]; 
	if(taxData.tags.length > 0) 
		tagsID = taxData.tags[0]; 
 
	//alert(taxData.geography[0]);
	
	//taxDialogDiv.dialog("close");
	if(categoryID == 0 && geographyID == 0 &&  tagsID == '')
	{
		document.getElementById('dgcontent_taxonomy_placeholder').innerHTML = '';
		$("#dgcontent_taxonomy_error").html('One of the options must be chosen');
		$("#dgcontent_taxonomy_error").show('fast');
		$("#dgcontent_taxonomy_popup").animate({scrollTop: $("#dgcontent_taxonomy_error").offset().top},'slow');

		return false;
	}

	 $.ajax({
		type: 'POST',
		url: 'index.php?eID=dg_content_ajax&lang_uid=0&lang_iso=',
		data: {fnc_name:'getFilterLink',category:categoryID,geography:geographyID,tags:tagsID,catPidList:11464},
		success: function(data){
					var locationLink = redirectLink + data;
					location.href=locationLink;
				},
		error: function (data, status, e)
			{
				alert(status);
				alert(e);
			}
	});
}

</script>

