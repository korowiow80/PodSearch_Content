<script type="text/javascript">
    jQuery(function() { jQuery('#mainContents').prepend(jQuery('#breadcrumb')); });
    //show/hide logout and login links
    function generateUserInfo () {
        var cust = ""
        if(cust !== "") {
            //jQuery('#mainheader li.invisible').removeClass('invisible').addClass('showing');
            //jQuery('#mainheader li:first, #mainheader li:eq(1)').addClass('invisible').removeClass('showing');
            jQuery('#shopping-header span.invisible').removeClass('invisible').addClass('showing');
            jQuery('#shopping-header span:first').addClass('invisible').removeClass('showing');
        }
if ( jQuery('#tab48').show) { jQuery('#tab48').remove(); }    //Remove ASI tab from website
}
    generateUserInfo();

    /** MOVE DROPDOWNS **/
    (function() {
        var navBtns = jQuery('#navbar').children().first().children(':not(#tab43):not(#tab45)');
        jQuery('#dropdowns-container')
        .children().hide()
        .each(function(i) {
            jQuery(this).appendTo(navBtns.eq(i));
        });
        navBtns.hover(function() { jQuery(this).children('.children').toggle(); });
        if ( jQuery('#tab43').hasClass('active') ) { jQuery('#tab1').addClass('active'); }
    })()
</script>
