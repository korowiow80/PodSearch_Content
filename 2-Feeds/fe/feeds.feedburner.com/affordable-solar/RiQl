<script type="text/javascript">
    (function(){
        function appendNav(lvl){
            var currentLevel = jQuery("#leftnav").find("li:has(.level-" + lvl + ")"), prevLevel = currentLevel.prev("li:has(.level-" + (lvl - 1) + ")").append("<ul class='lvl-" + (lvl - 1) + "'></ul>");
            currentLevel.find("a").removeClass("level-" + lvl);
            currentLevel.each(function(){
                jQuery(this).appendTo(jQuery(this).prevAll(":has(.lvl-" + (lvl - 1) + "):first").find("ul:first"));
            });
            lvl--;
            if (lvl > 0) 
                appendNav(lvl);
        }
        var leftSrc = jQuery("#leftcolumnsource").hide();
        jQuery('#navDestination').append('<ul id="leftnav"/>');
        var leftNav = jQuery("#leftnav"), navLvl = 0, currentLvl = 0;
        leftSrc.find("a:not(:has(img)):not(:empty)").addClass(function(){
            currentLvl = (jQuery(this).parent().parent().find("td").length + jQuery(this).parent().parent().find("td:first").attr("colspan") - 2);
            navLvl = (navLvl < currentLvl) ? currentLvl : navLvl;
            return "level-" + currentLvl;
        }).each(function(){
            jQuery(this).clone().appendTo(leftNav).wrap('<li title="' + jQuery(this).text() + '"></li>');
        })
        leftNav.find(".textboldnolink").removeClass("textboldnolink").parent().addClass("active");
        appendNav(navLvl);
        leftNav.find("li:has(.active)").addClass("active");
        leftNav.find('li:contains(New Releases)').attr('id', 'newReleases').children('a').first().attr('href', 'http://shopping.netsuite.com/s.nl/c.394134/sc.14/category.37674/ctype.SS/SS.37674/.f');
    })();
</script>
