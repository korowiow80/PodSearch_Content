	$(function() {
		// -- Fixes the backgrounds since by CSS was even more difficult 
		if($('.lvl-4').children('.active').length > 0) {
			$('.lvl-1').children('li').children('a').css('background-color', '#FFF');
			$('.lvl-2').children('li').children('a').css('background-color', '#FFF');
			$('.lvl-3').children('li').children('a').css('background-color', '#FFF');
			$('.lvl-4').children('li.active').children('a').css('background-color', '#dde3e4');			
		} else if($('.lvl-3').children('.active').length > 0) {
			$('.lvl-2').children('li').children('a').css('background-color', '#FFF');
			$('.lvl-1').children('li').children('a').css('background-color', '#FFF');
			$('.lvl-3').children('li.active').children('a').css('background-color', '#dde3e4');
		} else if($('.lvl-2').children('.active').length > 0) {
			$('.lvl-1').children('li').children('a').css('background-color', '#FFF');
			$('.lvl-2').children('li.active').children('a').css('background-color', '#dde3e4');
		}
		
		// -- Sets the event to turn them to bold, impossible only with CSS
		$('.lvl-2 li a').hover(function() {
		    $(this).css('font-weight', 'bold');
		}, function() {
		    $(this).css('font-weight', 'normal');
		});
	});
</script>

