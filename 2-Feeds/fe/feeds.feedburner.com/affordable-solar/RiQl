<script type="text/javascript">
    $('#itemlist').find('.itemcell:not(:has(".addtocart"))').find('.addtocart-lnk').hide();
    $('#itemlist .addtocart-lnk').click(function() {
        $(this).next().find('.addtocart').click();
        return false;
    });
    jQuery(function() {
        var pagi = jQuery('#mainContents').find('.pagination-results');
        if ( pagi.length > 0 ) {
            pagi = pagi.closest('table').css('float', 'right');
            jQuery('#description').after(pagi.first()).hide();
            pagi.last().closest('td[align=center]').children().first().css('float', 'right');
        }
        jQuery('#itemlist').removeClass('invisible');
    });

    /* REMOVE ERROR DATA FROM INFO ITEMS */
    jQuery('#itemlist').find('.itemcell').filter(':contains(Error)').find('.addcart, .pdf-link, .info-number').hide();

    jQuery('#itemlist').find('h1').text('Search Results');
</script>

</tr>
