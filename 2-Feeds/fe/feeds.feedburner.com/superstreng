<script type="text/javascript">
jQuery(function() {
    jQuery('#slideshow .inner').cycle({
		fx:     'scrollHorz', 
        speed:   1000, 
        timeout: 0, 
		prev:    '#prev',
		next:    '#next',
        pager:      '#nav',
        pagerEvent: 'mousedown'
    });
jQuery('#slideshow li a').mouseover(function() {
	jQuery('.inner').css('overflow','visible'); 
	
	jQuery(this).parent().find('span').css('display','block'); 
	
	offsetwidth = -(jQuery(this).parent().find('span').outerWidth()/2 - jQuery(this).outerWidth()/2 );
	//alert(offsetwidth)
	jQuery(this).parent().find('span').css('top','-48px');
	jQuery(this).parent().find('span').css('left',offsetwidth); 
})
jQuery('#slideshow li a').mouseleave(function() {
	jQuery('.inner').css('overflow','hidden');
	jQuery(this).parent().find('span').css('display','none'); 
})
});
</script>

