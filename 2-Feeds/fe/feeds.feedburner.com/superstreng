<script type="text/javascript">
jQuery(document).ready(function(){

jQuery("input#s").bind("blur",function() {
if(jQuery(this).val() == '') {
jQuery(this).val('Søk i bloggen');
}
});
jQuery("input#s").bind("focus",function() {
if(jQuery(this).val() == 'Søk i bloggen') {
jQuery(this).val('');
}
});

});
</script>

