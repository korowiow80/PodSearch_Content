<script type="text/javascript" src="/js/jquery.tablesorter.min.js" charset="utf-8"></script> 
<script>
jQuery(function($) 
    { 
		$.tablesorter.addParser({
			id:'rpm',
			is:function(s) {
				return false;
			},
			format:function(s){
				s = s.toLowerCase().replace(/ /g,"").replace(/rpm/ig,"");
				var sArr = s.split(';');
				for(var i=0;i<sArr.length;i++) {
					sArr[i] = +sArr[i];
				}
				sArr.sort(function(a, b) {
					return b - a;
				});
				return sArr[0];
			},
			type:'numeric'
		});
		$.tablesorter.addParser({
			id:'capacity',
			is:function(s) {
				return false;
			},
			format:function(s) {
				s = s.toLowerCase().replace(/ /g,"").replace(/gb/ig,"");
				var sArr = s.split(';');
				for(var i=0;i<sArr.length;i++) {
					sArr[i] = +sArr[i];
				}
				sArr.sort(function(a, b) {
					return b - a;
				});
				return $.tablesorter.formatFloat(sArr[0]);
			},
			type:'numeric'
		});
        $("#ProductListTable").tablesorter({
			headers: {
				0:{ sorter: false},
				1:{sorter:'rpm'},
				2:{sorter:'capacity'}
			}
		}); 
    } 
);
</script>
