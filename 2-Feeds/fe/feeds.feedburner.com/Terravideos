      <title>TERRA 529: Angels of the Forest: Silky Sifaka Lemurs of Madagascar PART TWO</title>
      <blip:user>terraadmin</blip:user>
      <blip:userid>32045</blip:userid>
      <blip:safeusername>terraadmin</blip:safeusername>
      <blip:showpath>terra-the-nature-of-our-world</blip:showpath>
      <blip:show>TERRA: The Nature of Our World</blip:show>
      <blip:showpage>http://blip.tv/terra-the-nature-of-our-world</blip:showpage>
      <blip:picture>http://a.images.blip.tv/Terraadmin-picture902.jpg</blip:picture>
      <blip:posts_id>3068909</blip:posts_id>
      <blip:item_id>3049269</blip:item_id>
      <blip:item_type>file</blip:item_type>
      <itunes:explicit>no</itunes:explicit>
      <blip:rating>0.0</blip:rating>
      <blip:datestamp>2010-01-05T18:28:16Z</blip:datestamp>
      <blip:language>English</blip:language>
      <blip:adChannel>Documentary</blip:adChannel>
      <blip:recommendations>0</blip:recommendations>
      <blip:recommendable>0</blip:recommendable>
      <blip:core>1</blip:core>
      <blip:core_value>9</blip:core_value>
      <blip:channel_name>Learning</blip:channel_name>
