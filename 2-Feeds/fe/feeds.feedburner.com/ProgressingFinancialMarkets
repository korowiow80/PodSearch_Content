jQuery(document).ready( function(){
	jQuery('#CPE-Hero-Image-Portlet a[href*="http://www.ibm.com/ibm/green/index5.shtml"]').click( function(){
		window.open(this.href); 
		return false;
	});
});

jQuery(function(){
	jQuery('a[rel*="blank"]').click(function(){
		return !window.open(jQuery(this).attr('href'));
	});
});

