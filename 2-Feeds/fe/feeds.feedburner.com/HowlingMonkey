    <item>
      <pubDate>Thu, 12 Apr 2012 07:34:00 -0700</pubDate>
      <title>Rylyeh Chronicles '12: #34</title>
      <link>http://feedproxy.google.com/~r/HowlingMonkey/~3/0P98GzCbgO0/rylyeh-chronicles-12-34</link>
      <guid isPermaLink="false">http://thehowlingmonkey.posterous.com/rylyeh-chronicles-12-34</guid>
