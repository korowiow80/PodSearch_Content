		<dc:creator>info@radio-gamba.nl (Famous Bobby T)</dc:creator>
				<category><![CDATA[Theeshow]]></category>
		<category><![CDATA[bionisch]]></category>
		<category><![CDATA[debutant]]></category>
		<category><![CDATA[Gans Gak]]></category>
		<category><![CDATA[gedichtenpik]]></category>
		<category><![CDATA[kersttijd]]></category>
		<category><![CDATA[kinderboerderij]]></category>
