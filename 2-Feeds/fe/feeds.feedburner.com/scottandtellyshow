<script type="text/javascript">

	  ps.event.dom_ready(function() {
          setTimeout(function() {
	    var tpl = '{if $que && $media}'+
	              '{capture assign="signup_uri_part"}?mId={$media.id}&amp;{$que.key}={$que.id}{/capture}'+
	              '{/if} Please login below or <a href="/signup/{$signup_uri_part}" rel="nofollow">Register</a>';
	    ps.tpls.set('_login-msg', tpl);
	    ps.message.add({'now-playing': function(d) {if (ps.dom.gbi('join-msg')) ps.tpls.render('_login-msg', d, 'join-msg');}});
		ps.message.add({'post_register-action': function(d) {
			var html = ps.dom.gbi('join-msg').innerHTML;
			var url = html.match("href=\"([^\"]+)\"");
			if(url) {
				url = url[1].replace(/&amp;postReg=.*/, '');
				d.postReg && (url += '&amp;postReg=' + d.postReg);
				ps.dom.gbi('join-msg').innerHTML = html.replace(/href="([^"]+)"/, "href=\""+url+"\"");
			}
			//console.log('rendered login');
		}});
}, 1);
          });

</script>


