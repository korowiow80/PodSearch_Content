<div><img id="crux" name="crux" height="1" width="1" src="/images/spacer.gif?js=off" alt=""></div>

<script type="text/javascript">

__crux_fpv = "";
__crux_pvt = "";
__crux_docref = document.referrer;

(function() {
  if (typeof swfobject != "undefined") {
      var __fpv = swfobject.getFlashPlayerVersion();
      __crux_fpv = __fpv.major + "-" + __fpv.minor + "-" + __fpv.release;
  }
  var c = "0123456789ABCDEFGHIJKLMNOPQRSTUVWXTZabcdefghiklmnopqrstuvwxyz";
  var l = 32;
  var tok = "";
  for (var i=0; i<l; i++) {
    var rnum = Math.floor(Math.random() * c.length);
    tok += c.substring(rnum,rnum+1);
    __crux_pvt = tok
  }
  if (typeof __crux_docref == "string") {
    __crux_docref = __crux_docref.substring(0,100);
  }
})();

function omniture_track(theMedia){
	if (typeof s_gi == "undefined"){
		omniture_track.stack.push(theMedia);
		return;
	}
	return omniture_track_i(theMedia);
}

omniture_track.stack = [];

function omniture_track_i(theMedia){
	var i=theMedia.indexOf("//");
	i+=2;
	var j=theMedia.indexOf("/",i);
	var domain=theMedia.substring(0,j);
	var query=theMedia.substring(j);
	var s=s_gi(s_account);
	s.linkTrackVars="prop4,pageName,prop18";
	s.linkTrackEvents="None";
    s.trackExternalLinks=true;
    if (typeof(internalFilters != "undefined")) {
        s.linkInternalFilters = internalFilters;
    }
	var channel;
	switch(domain){
	case "http://m.podshow.com":
	case "http://m-uk.podshow.com":
		channel="pmt---"+domain+"----podshow show";
		break;
	case "http://music.podshow.com":
		channel="pmt---"+domain+"----podshow song";
		break;
	default:
		if(domain.indexOf("podshow.com")>=0) {
			channel="pmt---"+domain+"----podshow media";
		}else {
			channel="pmt---"+domain+"----external media";
		};
		break;
	};
	s.prop4=channel;
	s.prop18="";
	s.pageName=s.getValOnce("pmt---"+theMedia,"omni_pdn_media_clicked",0);
	//s.events="event9";
	s.pageURL="";
	if(s.pageName){
		void(s.t());
	}
}

function log_crux ( query ) {
	u = ps.tpls.get_from_globals("current_user");
	if ( typeof(u) != "undefined" && u && typeof(u.user_id) != "undefined" && u.user_id ) {
			query = query+"&user_id="+u.user_id;
	}
	setTimeout("_log_crux('"+query+"')",0);
}

function _log_crux(query)
{
  var indx = ps.dom.gi();
  if (indx in window) {
 	//  log_crux(url); do not know
    return;
  }
  ps[indx] = new Image;
  ps[indx].onerror = ps[indx].onload = function() {
    delete ps[indx];
  }
  ps[indx].src=ps.track_url+"?crux_pvt="+__crux_pvt+"&crux_fpv="+__crux_fpv+"&crux_docref="+__crux_docref+"&"+query;
}

function log_facebook(action,d) {
    var query = "crux="+action;
    if (d && (typeof(d.media) != "undefined") && (typeof(d.media.id) != "undefined")) {
      query += "&mId="+d.media.id;
    }
    log_crux(query);
}

function log_crux_media(pixeltype,d,cur_cid,cur_list_id,cur_slot,crux_howplayed,crux_page_name,mtime) {
  // this function will fail if there is no "media" in d

  // flash player does its own logging
  if ( !("media" in d) || ("flv" in d.media.media_urls) || d.media.media_ext == "mp3" ) return;

  var cCall = "";
  var crux_list_id = -1;
  var mId = -1;
  if ((d) && (typeof(d.media) != "undefined") && (typeof(d.media.id) != "undefined")) mId = d.media.id;
  if (pixeltype!="end") pixeltype = "start";
  var stack = ["mId="+mId, "page="+crux_page_name, "crux=media_"+pixeltype];
  for (var k in d) {
    if (!d[k] || (d[k].constructor !== Object) || (k == "media"))
      continue;
    if ("key" in d[k])
      stack.push(d[k].key + "=" + d[k].id);
  }

  // placement tracking is speced for home page only
  if (mtime>0) stack.push("msecs="+mtime);
  cCall += stack.join("&");
  log_crux( cCall );
}

function howplayedstatekeeper() {
  var current_state = "unknown";

  this.process = process;
  this.get_howplayed = get_howplayed;

  function process(event_name,param) {
     if (event_name == "load") {
       current_state = "play-load";
     } else if (event_name == "finished-playing") {
       current_state = "play-next";
     } else if (event_name == "media-clicked") {
       current_state = "play-clicked";
     } else if (event_name == "channel-clicked") {
       current_state = "play-chan";
     } else if (event_name == "done-playing") {
       current_state = "unknown";
     }
  }
  function get_howplayed() {
     return current_state;
  }
}

(function () {
  ps.track_url = "http://crux.mevio.com/index.gif";
  var TTP = 0;
  var myhowplayedstatekeeper = new howplayedstatekeeper();
  var prev_cid = 0, cur_cid = 0;
  var prev_slot = -1, cur_slot = -1;
  var prev_list_id = 0, cur_list_id = 0;
  var cur_data;
  var crux_page_name = "home";


  ps.message.add({"play": function(d) {
      if ("channel" in d) {
        prev_cid = cur_cid;
        cur_cid = d.channel.id;
      }

      if (!("media" in d))
        return;
      cur_data = d;
      TTP = (new Date).getTime();
      if (d.media.id == 0)
        return;
      omniture_track(d.media.media_url);
   }});

   ps.message.add({"play": function(d) {
    if (d.media && d.media.type_id == 8 && d.media.type_name == "vid") {
      ps.json.remote("get", {url:"/mc/videos/flash/player/videoViews.php", vars:"videoID="+d.media.key_id} );
    }
    }});

  var MediaTime = 0;

  ps.message.add({"now-playing": function(b) {
	MediaTime = (new Date).getTime();
    var crux_howplayed=myhowplayedstatekeeper.get_howplayed();
    var mId = (b)?(("media" in b)?b.media.id:-1):-1;
	log_crux("ttp="+ (((new Date).getTime() - TTP)) + "&mId=");
    log_crux_media("start",b,cur_cid,cur_list_id,cur_slot,crux_howplayed,crux_page_name,0);
     }});

  ps.message.add({"now-playing": function(b) {
	log_crux("time_from_load_to_play="+ (((new Date).getTime() - __ps_load_t)));
	ps.message.remove({"now-playing": arguments.callee});
      }});

  ps.message.add({"finished-playing": function(b) {  // happens when video is played ALLTHE WAY thru
    media_finished = 1;
    myhowplayedstatekeeper.process( "finished-playing" );
     }});

  ps.message.add({"done-playing": function(d) {  // can happen when switching from one video to another
	  MediaTime = (new Date).getTime() - MediaTime;
      myhowplayedstatekeeper.process( "done-playing" );
      var crux_howplayed=myhowplayedstatekeeper.get_howplayed();
      log_crux_media("end",d,prev_cid,prev_list_id,prev_slot,crux_howplayed,crux_page_name,MediaTime);
	  MediaTime = 0;
     }});

  ps.actions.add(function(a,params,e){
	if ("cId" in params) {
		myhowplayedstatekeeper.process("channel-clicked",params.cId);
	} else if ("mId" in params) {
		myhowplayedstatekeeper.process("media-clicked");
	}
    });

  ps.event.add(window, "load", function() {
      log_crux("page_load_time="+ (((new Date).getTime() - __ps_load_t)));
      myhowplayedstatekeeper.process( "load" );
    });

  ps.event.add(window, "beforeunload", function() {
      var mId = (cur_data)?(("media" in cur_data)?cur_data.media.id:-1):-1;
      if (mId > 0) {
	    MediaTime = (new Date).getTime() - MediaTime;
        myhowplayedstatekeeper.process( "done-playing" );
        var crux_howplayed=myhowplayedstatekeeper.get_howplayed();
        log_crux_media("end",cur_data,prev_cid,prev_list_id,prev_slot,crux_howplayed,crux_page_name,MediaTime);
        MediaTime = 0;
      }
    });

  ps.message.add({"facebook_posted_story" : function(d) { log_facebook("facebook_posted_story",d); } });
  ps.message.add({"facebook_posted_comment" : function(d) { log_facebook("facebook_posted_comment",d); } });

})()


</script>
<noscript><div><img src="http://crux.mevio.com/index.gif?crux=noscript" alt="crux"></div></noscript>

