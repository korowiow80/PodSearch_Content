      <title>EP 204: Hip Rolls on the BoSU</title>
      <blip:user>pilatesonfifth</blip:user>
      <blip:userid>86050</blip:userid>
      <blip:safeusername>pilatesonfifth</blip:safeusername>
      <blip:showpath>pilates-on-fifth-video-podcast</blip:showpath>
      <blip:show>Pilates on Fifth Video Podcast</blip:show>
      <blip:showpage>http://blip.tv/pilates-on-fifth-video-podcast</blip:showpage>
      <blip:picture>http://a.images.blip.tv/Pilatesonfifth-picture286.gif</blip:picture>
      <blip:posts_id>5029477</blip:posts_id>
