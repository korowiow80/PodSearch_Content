  &lt;tr height="160"&gt;
    &lt;td width="112" valign="top" height="160" align="left" colspan="3"&gt;&lt;img width="112" height="149" border="0" src="http://www.meridianmagazine.com/radio/images/cricketseagull_rust.gif"/&gt;&lt;/td&gt;
    &lt;td width="16" height="1536" rowspan="20"&gt;&lt;/td&gt;
    &lt;td width="224" valign="top" height="272" align="left" rowspan="3"&gt;&lt;img width="224" height="256" border="0" src="http://www.stevenkappperry.com/ronsimpsonpearl.jpg"/&gt;&lt;/td&gt;
