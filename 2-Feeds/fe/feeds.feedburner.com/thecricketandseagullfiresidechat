    &lt;td width="368" height="16" colspan="8"&gt;&lt;/td&gt;
    &lt;td width="17" height="1664" rowspan="22"&gt;&lt;/td&gt;
    &lt;td width="1" height="16"&gt;&lt;spacer width="1" height="16" type="block"&gt;&lt;/spacer&gt;&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr height="112"&gt;
    &lt;td width="368" valign="top" height="112" align="left" colspan="8"&gt;&lt;img width="368" height="96" border="0" src="http://www.meridianmagazine.com/radio/images/cs-logonew.jpg"/&gt;&lt;/td&gt;
    &lt;td width="1" height="112"&gt;&lt;spacer width="1" height="112" type="block"&gt;&lt;/spacer&gt;&lt;/td&gt;
