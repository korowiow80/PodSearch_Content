&lt;div&gt;&lt;span style="line-height: 20px;"&gt;&lt;span style="font-size: small;"&gt;&lt;span style="font-size: 12px;"&gt;&lt;br /&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;
