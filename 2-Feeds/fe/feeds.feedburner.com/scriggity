      <title>seattle snow = scriggity delay</title>
      <blip:user>scriggity</blip:user>
      <blip:userid>19912</blip:userid>
      <blip:safeusername>scriggity</blip:safeusername>
      <blip:showpath>scriggity</blip:showpath>
      <blip:show>scriggity</blip:show>
      <blip:showpage>http://blip.tv/scriggity</blip:showpage>
      <blip:picture>http://a.images.blip.tv/Scriggity-picture722-749.jpg</blip:picture>
      <blip:posts_id>133526</blip:posts_id>
      <blip:item_id>129804</blip:item_id>
      <blip:item_type>file</blip:item_type>
      <itunes:explicit>no</itunes:explicit>
      <blip:rating>0.0</blip:rating>
      <blip:datestamp>2007-01-11T06:34:38Z</blip:datestamp>
      <blip:language>English</blip:language>
      <blip:adChannel />
      <blip:recommendations>0</blip:recommendations>
      <blip:recommendable>0</blip:recommendable>
