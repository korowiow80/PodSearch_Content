function toggleLogin()
{
 var obj = getObject("login_box");
 if (obj.style.visibility == "visible")
 {
  obj.style.visibility = "hidden";
  obj.style.display = "none";
 }
 else
 {
  obj.style.visibility = "visible";
  obj.style.display = "block";
  document.popup_login_form.email.focus();
 }
}
function headerLogin()
{
 var frm = document.popup_login_form;
 trimFields(frm);
 if ( isEmpty(frm.email.value) ) return fieldError(frm.email, "Please provide a username or valid email address");
 if ( isEmpty(frm.password.value) ) return fieldError(frm.password, "Please provide your password");
 return true;
}
</script>
