        <item>
            <!-- begin RSS 2.0 tags -->
            <title>Guest Interview: Francois Audouze, very old wines</title>
            <guid isPermaLink="false">http://recordings.talkshoe.com/TS-6709.mp3</guid>
            <pubDate>Fri, 12 Jan 2007 12:20:36 -0500</pubDate>
            <author>talkshoe-pcast@sneakemail.com (randulo and Wine Brands)</author>
            <link>http://feedproxy.google.com/~r/NewWineConsumer/~3/o64Rq_1lpsA/TS-6709.mp3</link>
            <enclosure url="http://recordings.talkshoe.com/TC-11888/TS-6709.mp3" length="12051958" type="audio/mpeg" />
