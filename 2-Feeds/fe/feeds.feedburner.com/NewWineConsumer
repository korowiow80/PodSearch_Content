        <item>
            <!-- begin RSS 2.0 tags -->
            <title>Mutineer Magazine with creator Alan Kropf</title>
            <guid isPermaLink="false">http://recordings.talkshoe.com/TC-11888/TS-139074.mp3</guid>
            <pubDate>Wed, 20 Aug 2008 12:00:17 -0400</pubDate>
            <author>talkshoe-pcast@sneakemail.com (randulo and Wine Brands)</author>
            <link>http://feedproxy.google.com/~r/NewWineConsumer/~3/XEfEWpYQLJs/TS-139074.mp3</link>
            <enclosure url="http://recordings.talkshoe.com/TC-11888/TS-139074.mp3" length="22845622" type="audio/mpeg" />
