&lt;tr style="height: 15.0pt; mso-yfti-irow: 1;"&gt;
  &lt;td nowrap="" style="background: white; border-right: solid black 1.0pt; border: none; height: 15.0pt; mso-background-themecolor: background1; mso-border-right-themecolor: text1; padding: 0in 5.4pt 0in 5.4pt; width: 114.0pt;" valign="top" width="152"&gt;&lt;div class="MsoNormal" style="margin-bottom: 0.0001pt;"&gt;
