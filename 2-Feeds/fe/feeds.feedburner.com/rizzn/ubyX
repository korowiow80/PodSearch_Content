<script type="text/javascript" src="http://yui.yahooapis.com/2.5.1/build/get/get-min.js" ></script> 
<script type="text/javascript">
// ///
// YAHOO.widget.DateMath 
// http://developer.yahoo.com/yui/docs/YAHOO.widget.DateMath.html 
// This is a great utility for doing date math in JavaScript.
// Reproduced here so we don't have to include the entire humongous YUI Calendar widget, 
// which is where this comes from
// we're only using YAHOO.widget.DateMath.add(), YAHOO.widget.DateMath.after() and their supporting functions
// ///
YAHOO.fcue.load = function() { 
	var YUIMinBuild = 733; // 2.5.2
	var YUICurrentVersion = "2.5.2";
	var YUIBasePath = "http://yui.yahooapis.com/" + YUICurrentVersion + "/build/";
	var fcueBasePath = "http://d.yimg.com/fx/fc/cues/Help/us/en/95/";
	var fcueJSPath = "http://d.yimg.com/fx/fc/cues/Help/us/en/95/fcue.js";
	var fcueCSSPath = "http://d.yimg.com/fx/fc/cues/Help/us/en/95/fcue.css";
	var userStateViewMax = 1;
	YAHOO.fcue.userStateCampaignEndDate = new Date(2009,9,2);

	var successHandler = function() { 
		var now = new Date();
		function saveStateAndInitCues(fcueData) { 
			var moreYUILibs = ['dom','event'];
			var moreLibURLs = [];
			for(var i=0;i<moreYUILibs.length;i++) {
				if(!YAHOO.env.getVersion(YUILibs[i])) {
					moreLibURLs.push(YUIBasePath + moreYUILibs[i] + "/" + moreYUILibs[i] + "-min.js");
				}
			}
		
			var getSuccessHandler = function(o) {
				YAHOO.fcue.preloadFcueImages(0);

				var leaving = true;
				YAHOO.fcue.checkURL = function (href,fcueData) {
					leaving = (((href.indexOf("://") > -1) || (href.indexOf("help.") > -1)) && (href.indexOf(window.location.host) == -1));
					return leaving;
				}
	
				YAHOO.fcue.showCue = function (o) {
					YAHOO.fcue.preloadFcueImages(1);
					YAHOO.fcue.setTheCookie(fcueData);
				}

				YAHOO.fcue.setTheCookie = function (o) {
					var fcueDataString = YAHOO.lang.JSON.stringify(o); 
                        		YAHOO.util.Cookie.set('fcue',fcueDataString, {
                                		expires: new Date(2009,9,2),
                                		path: "/"
                        		}); 
				}
			
				var e = document.getElementsByTagName('a');
				var curURL = window.location.href;
				var prop = curURL.split('/');
				if(curURL.indexOf('?terms=') != -1) {
					var keywords = curURL.substring(curURL.indexOf('?')+7, curURL.length);
					keywords = keywords.replace('#','');
				} else {
					var keywords = '';
				}
				var surveyURL = 'http://survey.confirmit.com/wix3/p705105329.aspx?parameter1='+prop[6]+'&parameter2='+curURL;
				if(keywords != '')
					surveyURL += '&parameter3='+keywords;
                                for(var i=0;i<e.length;i++) {
					if (YAHOO.fcue.checkURL(e[i].href)) {
						YAHOO.util.Event.addListener(e[i], 'click', function(e) {
							var originalURL = this.href;
							YAHOO.util.Event.preventDefault(e);
							YAHOO.fcue.showCue(true);
							YAHOO.util.Event.addListener("fcuebtn2", 'click', function() {
								location.href = originalURL;
							});
							YAHOO.util.Event.addListener("fcuebtn1", 'click', function() {
								window.open(surveyURL);
								location.href = originalURL; 
							});
						});
					}
				}	
			};	
		moreLibURLs.push(fcueJSPath); 
		YAHOO.util.Get.css(fcueCSSPath);
		YAHOO.util.Get.script(moreLibURLs, {onSuccess: getSuccessHandler, data: fcueData});
	}

	var fcueDataString =YAHOO.util.Cookie.get('fcue');
	if(fcueDataString) {
		var fcueData = YAHOO.lang.JSON.parse(fcueDataString);
	} if(!fcueData) { var fcueData = {};
		fcueData.viewCount = 1;
		saveStateAndInitCues(fcueData);
	} else {
		var view=true;
		if(fcueData.viewCount==userStateViewMax) {
			view=false;
		}
		if(view) {
			fcueData.viewCount = 1;
				saveStateAndInitCues(fcueData);
			} 
		}
	}
	var YUILibs = ['json','cookie'];
	var libURLs = [];
	for(var i=0;i<YUILibs.length;i++) {
		if(!YAHOO.env.getVersion(YUILibs[i])) {
			if(YUILibs[i]=="cookie") { 
				var beta="-beta";
			} else {
				var beta="";
			}
			libURLs.push(YUIBasePath + YUILibs[i] + "/" + YUILibs[i] + beta + "-min.js"); 
		}
	}
	if(libURLs.length==0) {
                successHandler();
        } else {
                YAHOO.util.Get.script(libURLs, {onSuccess: successHandler});
        }
}();
</script>
