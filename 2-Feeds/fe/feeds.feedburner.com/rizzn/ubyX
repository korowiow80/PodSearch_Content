	var usingRadioButtons = false;
	var yhelpPathId="/l/us/yahoo/music/music/aboutmusic/webjay.html";
	var yhelpVersionId = 249520;
	var yhelpFeedbackDefaultMessage = "The information you provide will be used to make this information more helpful in the future. We will not respond directly to your comments.";

	function adjustArcticleFontSize(scale)
	{
		if(scale == 'fss') {
			document.getElementById('articleBody').style.fontSize = '88%';
			document.getElementById('fss').className = "fss fsactive";
			document.getElementById('fsm').className = "fsm";
			document.getElementById('fsl').className = "fsl";
		}
		if(scale == 'fsm') {
			document.getElementById('articleBody').style.fontSize = '113%';
			document.getElementById('fss').className = "fss";
			document.getElementById('fsm').className = "fsm fsactive";
			document.getElementById('fsl').className = "fsl";
		}
		if(scale == 'fsl') {
			document.getElementById('articleBody').style.fontSize = '138%';
			document.getElementById('fss').className = "fss";
			document.getElementById('fsm').className = "fsm";
			document.getElementById('fsl').className = "fsl fsactive";
		}
	}
	function yhelpClearPreset()
	{
			var e = document.getElementById(".feedback");
			if(e.value == "" || e.value.replace(/\s/g, "") == yhelpFeedbackDefaultMessage.replace(/\s/g, ""))
			{
				e.value = "";
				e.style.color = "#404040";
			}
	}	
	function yhelpRecordVote(vote)
	{
		yhelpRequestUrl = "/js/recordclick.html";
		yhelpPostData = "crumb=" + encodeURIComponent(yhelpCrumb);
		yhelpPostData += "&vid=" + encodeURIComponent(yhelpVersionId);
		yhelpPostData += "&path=" + encodeURIComponent(yhelpPathId);
		yhelpPostData += "&vote=" + encodeURIComponent(vote);
		yhelpRequest = YAHOO.util.Connect.asyncRequest('POST', yhelpRequestUrl, yhelpCallback, yhelpPostData);
	}

	function yhelpRemoveVoting()
	{
		YAHOO.util.Event.removeListener("thumbsup", "click", yhelpThumbsUp);
		YAHOO.util.Event.removeListener("thumbsdown", "click", yhelpThumbsDown);
	}
	function yhelpThumbsUp(){
		if(!usingRadioButtons){
			document.getElementById('thumbsup').style.backgroundPosition = "right -379px";
		}
		document.getElementById('module2').style.display = 'block';
		yhelpRemoveVoting();
		yhelpRecordVote("yes");
}
  	function yhelpThumbsDown(){
		document.getElementById('module1').style.display = 'block';
		if(!usingRadioButtons){
			document.getElementById('thumbsdown').style.backgroundPosition = "right -466px";
		}
		yhelpRemoveVoting();
		yhelpRecordVote("no");
	}
	function yhelpRecordFeedback(e)
	{
		document.getElementById('feedbackConfirm').style.display = 'block';
		document.getElementById('feedbackArea').style.display = 'none';
		var feedback = "";
		var surveyResponse = "";
		if(document.getElementById('.feedback'))
		{
			feedback = document.getElementById('.feedback').value;
			if( document.getElementById('.feedback').value.replace(/\s/g, "") == yhelpFeedbackDefaultMessage.replace(/\s/g, ""))
			{
				feedback = "";
			}
		}
		if(document.getElementById('yhelpFeedbackSurvey1').checked)
		{
			surveyResponse = 1;
		}
		if(document.getElementById('yhelpFeedbackSurvey2').checked)
		{
			surveyResponse = 2;
		}
		if(document.getElementById('yhelpFeedbackSurvey3').checked)
		{
			surveyResponse = 3;
		}
		if(document.getElementById('yhelpFeedbackSurvey4').checked)
		{
			surveyResponse = 4;
		}
		if(document.getElementById('yhelpFeedbackSurvey5').checked)
		{
			surveyResponse = 5;
		}
		if(document.getElementById('yhelpFeedbackSurvey6').checked)
		{
			surveyResponse = 6;
		}
		yhelpPostData = "feedback=" + encodeURIComponent(feedback);
		yhelpPostData += "&surveyResponse=" + encodeURIComponent(surveyResponse);
		yhelpPostData += "&crumb=" + encodeURIComponent(yhelpCrumb);
		yhelpPostData += "&vid=" + encodeURIComponent(yhelpVersionId);
		yhelpPostData += "&path=" + encodeURIComponent(yhelpPathId);

		yhelpRequestUrl = "/js/recordclick.html";
		yhelpRequest = YAHOO.util.Connect.asyncRequest('POST', yhelpRequestUrl, yhelpCallback, yhelpPostData);

	}
	function yhelpRecordYesFeedback(e)
	{
		document.getElementById('yesThankYou').style.display = 'block';
		document.getElementById('feedbackArea2').style.display = 'none';
		var feedback = "";
		var surveyResponse = "";
		if(document.getElementById('yhelpYesFeedbackSurveyYesResponse').checked)
		{
			surveyResponse = "yes_ans_1_yes";
		}
		if(document.getElementById('yhelpYesFeedbackSurveyNoResponse').checked)
		{
			surveyResponse = "yes_ans_1_no";
		}
		yhelpPostData = "feedback=" + encodeURIComponent(feedback);
		yhelpPostData += "&surveyResponse=" + encodeURIComponent(surveyResponse);
		yhelpPostData += "&crumb=" + encodeURIComponent(yhelpCrumb);
		yhelpPostData += "&vid=" + encodeURIComponent(yhelpVersionId);
		yhelpPostData += "&path=" + encodeURIComponent(yhelpPathId);

		yhelpRequestUrl = "/js/recordclick.html";
		yhelpRequest = YAHOO.util.Connect.asyncRequest('POST', yhelpRequestUrl, yhelpCallback, yhelpPostData);

	}

	function init() {
		YAHOO.yhelp.overlay1 = new YAHOO.widget.Overlay("overlay1", { context:["myhlp","tr","tl"], visible:false, width:"375px" } );
		YAHOO.yhelp.overlay1.render();

		YAHOO.yhelp.overlay2 = new YAHOO.widget.Overlay("overlay2", { context:["myhlp","tr","tl"], visible:false, width:"375px" } );
		YAHOO.yhelp.overlay2.render();
		
		YAHOO.util.Event.addListener("myhelpbuttoncancel1", "click", YAHOO.yhelp.overlay1.hide, YAHOO.yhelp.overlay1, true);
		YAHOO.util.Event.addListener("myhelpbuttoncancel2", "click", YAHOO.yhelp.overlay2.hide, YAHOO.yhelp.overlay2, true);
		YAHOO.util.Event.addListener("myhelpbuttonsave", "click", yhelpSaveArticle, YAHOO.yhelp.overlay1, true);
		YAHOO.util.Event.addListener("myhelpbuttonreplace", "click", yhelpSaveArticle, YAHOO.yhelp.overlay2, true);
		YAHOO.util.Event.addListener("thumbsup", "click", yhelpThumbsUp);
		YAHOO.util.Event.addListener("thumbsdown", "click", yhelpThumbsDown);
		YAHOO.util.Event.addListener("saveBtn", "click", yhelpRecordFeedback);
		YAHOO.util.Event.addListener("yhelpYesFeedbackSurveyYesResponse", "click", yhelpRecordYesFeedback);
		YAHOO.util.Event.addListener("yhelpYesFeedbackSurveyNoResponse", "click", yhelpRecordYesFeedback);
		YAHOO.util.Event.addListener(".feedback", "click", yhelpClearPreset);
		YAHOO.util.Event.addListener(".feedback", "focus", yhelpClearPreset);
	}
