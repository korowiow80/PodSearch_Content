&lt;table width="490" border="0" cellspacing="1" cellpadding="5" align="center" bgcolor="black"&gt;
	&lt;tr height="130"&gt;
		&lt;td rowspan="2" align="center" bgcolor="#aaaaaa" width="80" height="207"&gt;
			&lt;h4&gt;&lt;strong&gt;Art&lt;br /&gt;
					of the&lt;br /&gt;
					week&lt;/strong&gt;&lt;/h4&gt;
		&lt;/td&gt;
		&lt;td rowspan="2" align="center" bgcolor="#e4e4e4" width="200" height="207"&gt;&lt;br /&gt;
			&lt;table border="0" cellspacing="0" cellpadding="1" bgcolor="black"&gt;
				&lt;tr&gt;
