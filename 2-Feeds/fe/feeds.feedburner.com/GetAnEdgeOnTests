<script type="text/javascript" language="javascript">
    

// new code -----------
var slidenum = 0;
var timerID = 0;
var pauseShow=0;
var secs=7;
var repeatshow = 1; // 1 = yes, 0 = no
var timedelay = 1000; // time in milliseconds, 15000 = `15 secs

var pnItem =new Array("mainleft_0_rpFeatures_ctl00_pnFeature","mainleft_0_rpFeatures_ctl01_pnFeature","mainleft_0_rpFeatures_ctl02_pnFeature","mainleft_0_rpFeatures_ctl03_pnFeature");
 
var slidecount = pnItem.length;

function pauseSlideShow() { pauseShow=1;};

function resetSlideShow() { 
                clearTimeout(timerID); 
                timerID = setTimeout('showSlideShow()', timedelay)
                
};
function showSlideShow() 
{
if (secs==0)
  {
    
    if(!pauseShow)
    {
           slidenum++;
       // alert('slide num:- '+ slidenum);
        if (slidenum < slidecount+1) 
        {
           // if the slide number is withing limit
           // call  the next slide
          // alert("next slide");
           setItemRight(slidenum-1,0);
        }
        if (slidenum == slidecount+1) 
        { // if finished
            if (repeatshow) 
            {
            //alert("Repeat Show");
                slidenum = 0;
                showSlideShow() 
            }
            //else 
        }
        else 
        {
       resetSlideShow();
        }
        
        
       
       
      } 
       secs=7;
  }
  else
    {
   // alert(" starting again");
        //self.status = secs + ".. SldNum "+ (slidenum) + " .. slide Count " + slidecount ;
        secs = secs - 1
        
        resetSlideShow();
    }
}





     function setItemLeft(sID,uInput) {
     
       try {
       pauseShow=0;
       // var pnItem =new Array("mainleft_0_rpFeatures_ctl00_pnFeature","mainleft_0_rpFeatures_ctl01_pnFeature","mainleft_0_rpFeatures_ctl02_pnFeature","mainleft_0_rpFeatures_ctl03_pnFeature");
 
        //alert("in setItemLeft " + sID); 
        for(i=0; i<pnItem.length; i++) {
        //alert(pnItem[i]); 
           var itemID = pnItem[i];   
           if ((itemID))
           document.getElementById(itemID).className = "invisible";
        }
        var j = sID - 1; 
        if(uInput)
        {
             slidenum--;
             if(slidenum<0) slidenum=pnItem.length-1;
             secs=7;
             resetSlideShow();
        }
        if (j < 0) j = (pnItem.length - 1);  
        if ((pnItem[j]))
        document.getElementById(pnItem[j]).className = "visible";
        
        }
        catch (err) {

            alert("Main: " + err.description);
        }
             return false; 
             
     }
     
     // ------ end of new code -
     function setItemRight(sID,uInput) {
     
       try {
       pauseShow=0;
        //alert("in setItemLeft " + sID); 
        for(i=0; i<pnItem.length; i++) {
        //alert(pnItem[i]); 
           var itemID = pnItem[i];   
           if ((itemID))document.getElementById(itemID).className = "invisible";
        }
        var j = sID + 1; // get next Item to the current counter
     if(uInput)  {
     
     slidenum++;
     if(slidenum>pnItem.length-1) slidenum=0;
     secs=7;
     resetSlideShow();
     }
        if (j > (pnItem.length - 1)) j = 0 ;  
        if ((pnItem[j]))
        document.getElementById(pnItem[j]).className = "visible";
        }
        catch (err) {

            alert("Main: " + err.description);
        }
             return false; 
             
     }

</script>
