<script type="text/javascript">    var searchkey = "Enter your search terms ...";
    var instructions = "Enter your search terms ...";
    function chkForTerm() {
        var txtBody = document._mainForm.searchkey.value;
        if (txtBody == "" || txtBody == instructions) {
            alert('Please enter a search term')
            return false;
        }
        else {
            return true;
        }
    }
    function srchFocus(searchField) {
        if (searchField.value != instructions) {
            searchField.style.color = 'Black';
        }
        else {
            searchField.value = "";
        }
    }
    function srchPress(searchField) {
        //alert("Enter srchPress(searchField, e)");
        if (searchField.value != instructions) {
            searchField.style.color = 'Black';
        }
       
    }
    

</script>
