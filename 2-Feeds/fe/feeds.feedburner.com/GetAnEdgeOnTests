    trackPageviewURL = getPath();

function getPath(url) 
{
    url = document.location.href;
    url = url.replace('http://', '');
    url = url.replace('https://', '');
    url = url.substr(url.indexOf('/'));
    url = url.split('?')[0];   
    
//    index   = url.indexOf('.aspx');
//    if (index != -1) url = url.slice(0, index + 5);
   
    return url;
}
