<script type="text/javascript" language="javascript">

    String.prototype.endsWith = function(str) { return (this.match(str + "$") == str) }
    /*
    ||
    ||  FUNCTION:  setTab
    ||
    ||
    ||  PURPOSE:   used to support the tab function, changes the class of page items 
    ||             based on the tab selected. 
    ||
    || Modification History:
    || Date        Developer           Description
    || ----------  ------------------  ---------------------------------------------
    || 09/16/2009  Timothy Fahy        Original version.   
    ||
    */
    function setTab(tabndex) {

        var debug = false;
        //try {
            var arr = new Array();
            arr = document.getElementsByTagName("div");
            for (var i = 0; i < arr.length; i++) {
                var itemID = arr[i].id;
     
                if (itemID.indexOf('tabLeft') == 0) {
                    if (itemID == ('tabLeft' + tabndex)) {
                        document.getElementById(itemID).className = "tabOnLeft";
                    }
                    else document.getElementById(itemID).className = "tabOffLeft";
                }
                if (itemID.indexOf('tabMiddle') == 0) {
                    if (itemID == ('tabMiddle' + tabndex)) {
                        document.getElementById(itemID).className = "tabOnMiddle";
                    }
                    else document.getElementById(itemID).className = "tabOffMiddle";
                }
                if (itemID.indexOf('tabContent') == 0) {
                    if (itemID == ('tabContent' + tabndex)) {
                        document.getElementById(itemID).className = "tabOnContent";
                    }
                    else document.getElementById(itemID).className = "tabOffContent";
                }
                if (itemID.indexOf('tabRight') == 0) {
                    if (itemID == ('tabRight' + tabndex)) {
                        document.getElementById(itemID).className = "tabOnRight";
                    }
                    else document.getElementById(itemID).className = "tabOffRight";
                }
                if (itemID.indexOf('pnDetail') > 0) {
                    if (debug) alert(itemID, "tab->" + tabndex);
                    if (itemID.endsWith(tabndex)) {
                        document.getElementById(itemID).className = "visible";
                        if (debug) alert(itemID + " tab->" + tabndex + " set-> content_row");
                    }
                    else {
                        document.getElementById(itemID).className = "invisible";
                        if (debug) alert(itemID + " tab->" + tabndex + " set-> invisible");
                    }
                }
                if (itemID.indexOf('pnItem') > 0) {

                    if (itemID.endsWith(tabndex)) {
                        document.getElementById(itemID).className = "visible";
                        if (debug) alert(itemID + " tab->" + tabndex + " set-> visable");

                    }
                    else {
                        document.getElementById(itemID).className = "invisible";
                        if (debug) alert(itemID + " tab->" + tabndex + " set-> invisible");
                    }
                }


            }
      //  }
      //  catch (err) {
//
   //         alert("Main: " + err.description);
   //     }

        return false;
    }

    function goToPage(url) {

        __doPostBack('goToArticle', url);

    }
    function doNotGoToPage() {

        alert("An article page URL has not been assigned to this item");
        return false;
    }

</script>

