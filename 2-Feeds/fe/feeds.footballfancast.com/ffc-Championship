$(document).ready(function(){

//rail test

function rail_check(){
$('#ve_ce_tgt').addClass('ve_ce_tgt');

if($('#ve_ce_tgt').hasClass('ve_ce_tgt')){
var rail_message = 'there is a rail!';
} else {
var rail_message = 'there is no rail!';
}

$('body').append('<!--'+rail_message+'-->');
}

setTimeout(rail_check,10000);

$('#scorecast_nav li.current_league ul').show();

$('.sc_navheader').click(function(){
if($(this).parent().hasClass('current_league')){
} else {
$('#scorecast_nav li').removeClass('current_league');
var theli = $(this).attr('rel');
$('#' +theli).addClass('current_league');
$('ul#scorecast_nav li ul').hide();
$('#' +theli + ' ul').show();
}
return false;
});
});
</script>


