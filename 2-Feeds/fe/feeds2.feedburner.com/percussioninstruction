      <link>http://feedproxy.google.com/~r/PercussionInstruction/~3/N8hiEVPJhTk/live-acoustic-kj-will-4693674</link>
      <title>Live Acoustic KJ &amp; Will</title>
      <blip:user>William Johnson</blip:user>
      <blip:userid>283202</blip:userid>
      <blip:safeusername>williamjohnson</blip:safeusername>
      <blip:showpath>percussion-instruction</blip:showpath>
      <blip:show>Percussion Instruction</blip:show>
      <blip:showpage>http://blip.tv/percussion-instruction</blip:showpage>
      <blip:picture>http://blip.tv/skin/blipnew/placeholder_user.gif</blip:picture>
      <blip:posts_id>4693674</blip:posts_id>
      <blip:item_id>4675406</blip:item_id>
      <blip:item_type>file</blip:item_type>
      <itunes:explicit>no</itunes:explicit>
      <blip:rating>0.0</blip:rating>
      <blip:datestamp>2011-01-24T15:11:23Z</blip:datestamp>
      <blip:language>English</blip:language>
      <blip:adChannel>Music</blip:adChannel>
      <blip:recommendations>0</blip:recommendations>
      <blip:recommendable>0</blip:recommendable>
      <blip:core>0</blip:core>
      <blip:channel_name>Music</blip:channel_name>
