					<param name='menu' value='false' />
					<param name='bgcolor' value='#000000' />
					<param name='wmode' value='opaque' />
									<span id="wp-as-964_45-container">
					<audio id='wp-as-964_45' controls preload='none'  style='background-color:#000000;width:290px;'>
						<span id="wp-as-964_45-nope">Download: <a href="http://www.medienengel.de/dbdt/audio/106_DasBueroDesTodes_Prinzip.mp3">106_DasBueroDesTodes_Prinzip.mp3</a><br /></span>
					</audio>
				</span>
				<br /><span id='wp-as-964_45-playing'></span>
				</object>			<script type='text/javascript'>
