					<param name='wmode' value='opaque' />
									<span id="wp-as-1048_33-container">
					<audio id='wp-as-1048_33' controls preload='none'  style='background-color:#000000;width:290px;'>
						<span id="wp-as-1048_33-nope">Download: <a href="http://www.medienengel.de/dbdt/audio/118_DasBueroDesTodes_Effizienz.mp3">118_DasBueroDesTodes_Effizienz.mp3</a><br /></span>
					</audio>
				</span>
