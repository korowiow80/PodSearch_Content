            <guid isPermaLink="false"><![CDATA[http://gscoas.libsyn.com/index.php?post_id=187212#]]></guid>
            <media:thumbnail url="http://assets.libsyn.com/item/1155011" />
            <pubDate>Wed, 28 Feb 2007 18:13:00 +0000</pubDate>
            <itunes:explicit>clean</itunes:explicit>
            <itunes:keywords />

