			<script type="text/javascript">
				function webradio_window (id) {
					fenster = window.open("http://www.energy.de/webradio/index.php?play="+id, "fenster1", "width=950,height=700,status=no,scrollbars=auto,resizable=no");
					fenster.focus();
				}
				function oeffnefenster (url) {
					fenster = window.open(url, "fenster1", "width=950,height=700,status=no,scrollbars=auto,resizable=no");
					fenster.focus();
				}
			</script>
