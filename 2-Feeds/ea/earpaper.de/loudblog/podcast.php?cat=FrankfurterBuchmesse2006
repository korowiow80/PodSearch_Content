<item>
    <pubDate>Wed, 15 Oct 2008 01:29:00 +0200</pubDate>
    <title>Frankfurter Buchmesse Audio-Podcast #14.Oktober 2008</title>
    <link>http://www.earpaper.de/loudblog/index.php?id=1081</link>
    <guid>http://www.earpaper.de/loudblog/index.php?id=1081</guid>
    <dc:creator>earpaper.de</dc:creator>
    <itunes:author>earpaper</itunes:author>
    <itunes:explicit>no</itunes:explicit>
    <itunes:keywords>Frankfurter Buchmesse 2006, Frankfurter Buchmesse</itunes:keywords>
    <category>Frankfurter Buchmesse 2006</category>
    <category>Frankfurter Buchmesse</category>
