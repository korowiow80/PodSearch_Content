	<div id="scheduleSummary" class="groupItemNarrow">
		<script type="text/javascript">
	jQuery(document).ready(function(){
	
		/*jQuery('#schedule').accordion({

			animated: false,
			autoHeight: false
   		});*/

		jQuery('div#scheduleSummaryArea a.head').next('div').children('table.scheduleSummary').hide();
		jQuery('div#scheduleSummaryArea a.head:first').next('div').children('table.scheduleSummary').show();

   		var lastBlock = jQuery('div#scheduleSummaryArea').children('a.head:first');
    
   		jQuery('div#scheduleSummaryArea a.head').click(
			function(){
				var targetContent = jQuery(this).next('div').children('table.scheduleSummary');
				
				jQuery(lastBlock).next('div').children('table.scheduleSummary').hide();
				
				lastBlock = this;
			
				if (targetContent.css('display') == 'none') {
					targetContent.show();
				} else {
					targetContent.hide();
				}
			}
		);
	});
</script>
