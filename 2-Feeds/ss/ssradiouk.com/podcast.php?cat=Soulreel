		<script type="text/javascript">
	jQuery(document).ready(function(){

		jQuery('#recentlyArchivedShowTabs > ul').tabs({
			cache: true,
			spinner: 'Loading...'
		});
	});

	var player = null;

	function playerReady(obj) {
   		player = document.getElementById(obj.id);
      		
		player.addModelListener("STATE", "stateTracker");
/* 		player.addModelListener('TIME', 'monitorIntro'); */
	};
	
	function stateTracker(obj) {
		if (obj.newstate == "PLAYING") {
/*
			*/

			jQuery.post(
				"http://ssradio.com/wp-content/themes/ssradio/tweet.php",
				{ postID: obj.id, userID: "0" },
				function(data){
/*
					*/
				},
				"json"
			);
	
      		player = document.getElementById(obj.id);
      		
  			player.removeModelListener("STATE","stateTracker");
		}
	};

	function monitorIntro(obj) {
   		player = document.getElementById(obj.id);

		time = obj.position;

		if (obj.id == 'liveplayer') {
			starttime = 3.7;
			endtime = 4;
		}
		else {
			starttime = 4.5;
			endtime = 4.8;
		}
		
		if (time > starttime && time < endtime) {
			player.sendEvent('PLAY');
			player.sendEvent('PLAY');
		}
	};
</script>
