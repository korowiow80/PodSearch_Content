    <script type="text/javascript">
      $(document).ready(function() {
      getLanguages('10');
      doLanguage('10','313');
      //$("#S").attr('disabled', 'disabled');
      $("#S").click(goMP3);
      });
      function doLanguage(lang, ver) {
        $("#mp3download-version").attr('disabled', 'disabled');
        if (ver) ver = "&ver="+ver;
        else ver = "";
        $.ajax({
          type: "GET",
          url: "/block_language.php",
          data: "action=Versions&lang_name="+lang+ver,
          success: function(msg){
            $("#versions").empty().append(msg);
            $("#S").removeAttr('disabled');
            $("#mp3download-version").removeAttr('disabled');
            /** Specify styles of select boxes here, because of Safari **/
            $("#mp3download-version").attr('style', 'font-size: 11px; overflow: hidden;');
          }
        });
      }

      function getLanguages(sel) {
        $.ajax({
          type: "GET",
          url: "/block_language.php",
          data: "action=Languages&selected="+sel,
          success: function(msg){
            $("#languages").empty().append(msg);
            $("#mp3download-lang").change(function() {
              doLanguage($(this).attr('value'));
            });
            /** Specify styles of select boxes here, because of Safari **/
            $("#mp3download-lang").attr('style', 'font-size: 11px; overflow: hidden;');
          }
        });
      }

      function goMP3() {
        var v = $("#mp3download-version").attr('value');
        var l = $("#mp3download-lang").attr('value');
        var l = $("#mp3download-lang").val();
        //console.log(l,v);
        location.href = '/store/languageconfig?selected='+l+'&ver='+v;
        //$("#language").submit();
      }
    </script>   
