<script type="text/javascript">
var rotate;
$(document).ready(function(){
	$('.ad_container').each(function(item)
	{
		if(item != 0)
		{
			$(this).hide();
		}
	});
	if($('.ad_container').size() > 1)
	{
		rotate_banner();
	}
	
});

var cnt	= 0;

function rotate_banner()
{
	$('.ad_container:eq('+cnt+')').fadeOut();
	cnt++;
	if(cnt == $('.ad_container').size())
	{
		cnt = 0;
	}
	$('.ad_container:eq('+cnt+')').fadeIn(function(){
		setTimeout("rotate_banner()", 5000);
	});
}
</script>


