<script type="text/javascript">
/* <![CDATA[ */
	$(document).ready(function() {
	
	var check_anzahl_mi = function () {
		var anzahl_markindikatoren = $('#headerTickerxx123123_sw_tbl_markt_indikatoren').find('tr').length;
		anzahl_markindikatoren -= 1; /*da tabellenueberschrift*/
		
		if (anzahl_markindikatoren == 6){
			$('#headerTickerxx123123_sw_mi_suche').hide();
		} else {
			$('#headerTickerxx123123_sw_mi_suche').show();
		}
	}
	
	check_anzahl_mi();


	customPlaceholderText($("#QueryInput"), "Name/ISIN/WKN");
	customPlaceholderText($("#QueryInput2"), "Stichwort");

	$(".headerTickerxx123123_sw_show_dialog").click(function() {

	$(function() {
		// a workaround for a flaw in the demo system (http://dev.jqueryui.com/ticket/4375), ignore!
		$( "#headerTickerxx123123_sw_dialog-modal:ui-dialog" ).dialog( "destroy" );
	

	
		$( "#headerTickerxx123123_sw_dialog-modal" ).dialog({
			width: 800,
			modal: true
		});
	
	});
    
	$('.ui-widget-header').css('display','none');
      $('.headerTickerxx123123_sw_a_close_popup').blur();
     
     return false;
   });
	
	$(".headerTickerxx123123_sw_btn_cancel").click(function() {
		$("#headerTickerxx123123_sw_dialog-modal").dialog('close');
	});

   	$(".headerTickerxx123123_sw_a_close_popup").click(function() {

		$("#headerTickerxx123123_sw_dialog-modal").dialog('close');
	});

	var styleTable = function (){
		check_anzahl_mi(); /*nach jeder veraenderung pruefen, ob das suchfeld noch eingeblendet werden soll*/
		var count = 0;
        var count_ges =  $('#headerTickerxx123123_sw_tbl_markt_indikatoren').find('tr').length;
        $('#headerTickerxx123123_sw_tbl_markt_indikatoren').find('tr').each(function(){
		if(count % 2 == 0){
					$(this).addClass('odd');
		} else {
					$(this).removeClass('odd');

		}
		if(count == 1){

			$(this).find('td').each(function(){
				$(this).addClass('first');
				$(this).find('.upup').remove();
		});
		} else if (count == count_ges - 1) {
			$(this).find('td').each(function(){
				$(this).find('.downdown').remove();
			});
		} else {
			$(this).find('td').each(function(){
				$(this).find('input.upup').remove();
				$(this).find('input.downdown').remove();

				
				if ($(this).hasClass('column-up')){
					$(this).parent().find('.column-up').append('<input type="image" title="Hoch" class="upup" src="/images/newdesign/non.gif" style="width:10px;border-width:0px;" />');
				}
				if ($(this).hasClass('column-down')){
					$(this).parent().find('.column-down').append('<input type="image" title="Runter" class="downdown" src="/images/newdesign/non.gif" style="width:10px;border-width:0px;" />');
				}
				
				$(this).removeClass('first');
			});
		}

		count++;
       	}); 
	};

    $("#headerTickerxx123123_sw_tbl_markt_indikatoren .upup, #headerTickerxx123123_sw_tbl_markt_indikatoren .downdown").live('click',function(){
        var row = $(this).closest('tr');
        if ($(this).is(".upup")) {
            row.insertBefore(row.prev());
        } else {
            row.insertAfter(row.next());
        }
        styleTable();
    });
    
    $(".removeTableRow").live('click',function(){
		var row = $(this).parents("tr:first");
		row.remove();
        styleTable();
    });
    
    var headerTickerxx123123_sw_AGS = '';
    
	var save_sorted_ags = function(){
		$.ajax({
				url:'/parts/boxes/marktindikatoren/process_data.m',
				data: {
					ag_list:		headerTickerxx123123_sw_AGS,
					user:			'sss',
					COMPONENT_ID:	'headerTickerxx123123_sw',
					indicator_name: 'HeaderTicker'
				},
				success: function(data){
				}
			});	
	};
	
	var do_search = function(SuchBegriff){
		var SearchName = $('.headerTickerxx123123_sw_inpt_searchname').val();
				$.ajax({
				url:'/parts/boxes/marktindikatoren/process_search.m',
				data: {
					searchname:		SuchBegriff,
					COMPONENT_ID:	'headerTickerxx123123_sw'
				},
				success: function(data){
					var result = $.trim(data);
					if (result == 'noMatch'){
						$('.headerTickerxx123123_sw_dv_noresult').show('fast');
					} else {
						var csv_data = result.split(',');
						 $('#headerTickerxx123123_sw_tbl_markt_indikatoren tr:eq(1)').before('<tr><td title="DAX"><a onclick="javascript:self.parent.location=&quot;/DE/index.aspx?pageID=82&amp;ISIN=DE0008469008&quot;" name="' + csv_data[0] + '">' + csv_data[1] + '</a></td><td class="column-up"></td><td class="column-down"><input type="image" title="Runter" class="downdown" src="/images/newdesign/non.gif" style="width:10px;border-width:0px;" /></td><td class="column-delete lastColOfRow"><input type="image" title="Entfernen" class="remove removeTableRow" src="/images/newdesign/non.gif" style="width:15px;border-width:0px;" /></td></tr>');
					}
			        styleTable();
				}
			});	
	};
    
	$(".headerTickerxx123123_sw_btn_submit").click(function(){
		var z = 0;
		$('#headerTickerxx123123_sw_tbl_markt_indikatoren').find('tr').each(function(){
			if(z > 0){
				if ($(this).find('a').attr('name')){
					headerTickerxx123123_sw_AGS += $(this).find('a').attr('name') + ', ';
				};
			};
			z++;
		});
		save_sorted_ags();
		setTimeout(function(){this.document.location.reload();}, 4000);
		$("#dialog-modal").dialog('close');

	});
	
	$(".btn_search_headerTickerxx123123_sw").click(function(){
		var SearchName = $('.headerTickerxx123123_sw_inpt_searchname').val();
		do_search(SearchName);
	});
	
	
	
	
});
/* ]]> */
</script>

	
