            <media:thumbnail url="http://assets.libsyn.com/item/1234894" />
            <pubDate>Wed, 18 Jan 2006 22:06:00 +0000</pubDate>
            <itunes:explicit>no</itunes:explicit>
            <itunes:keywords />

