            <pubDate>Fri, 13 Oct 2006 20:20:00 +0000</pubDate>
            <itunes:explicit>yes</itunes:explicit>
			<itunes:keywords>story,humor,whales,ships,sailing,telling,mermaid,seas,nautical</itunes:keywords>
            <itunes:subtitle />
