<script language="javascript">
function ShowHideURL(the_id){
	if ( document.getElementById("DivURL_"+ the_id).style.display	== '' ){
		document.getElementById("DivURL_"+ the_id).style.display	= 'none';
		document.getElementById("ImgURL_"+ the_id).src				= './templates/ModelSkin/images/tree1.gif';
	}
	else{
		document.getElementById("DivURL_"+ the_id).style.display	= '';
		document.getElementById("ImgURL_"+ the_id).src				= './templates/ModelSkin/images/tree0.gif';
	}
	return false;
}

function changeRadio(radio_file, link_id){
	var str_object	= '		<OBJECT id=winMediaPlayerID codeBase=http://activex.microsoft.com/activex/controls/mplayer/en/nsmp2inf.cab#Version=6,4,5,715 width=450 height=310 type=application/x-oleobject standby="Loading Microsoft Windows Media Player components..." classid=CLSID:6BF52A52-394A-11D3-B153-00C04F79FAA6 name=winMediaPlayer >';
	str_object		+= '		<PARAM NAME="URL" VALUE="'+ radio_file +'">';
	str_object		+= '		<PARAM NAME="rate" VALUE="1">';
	str_object		+= '		<PARAM NAME="balance" VALUE="0">';
	str_object		+= '		<PARAM NAME="currentPosition" VALUE="0">';
	str_object		+= '		<PARAM NAME="defaultFrame" VALUE="0">';
	str_object		+= '		<PARAM NAME="playCount" VALUE="1">';
	str_object		+= '		<PARAM NAME="CursorType" VALUE="-1">';
	str_object		+= '		<PARAM NAME="autoStart" VALUE="1">';
	str_object		+= '		<PARAM NAME="currentMarker" VALUE="0">';
	str_object		+= '		<PARAM NAME="invokeURLs" VALUE="-1">';
	str_object		+= '		<PARAM NAME="volume" VALUE="100">';
	str_object		+= '		<PARAM NAME="mute" VALUE="0">';
	str_object		+= '		<PARAM NAME="stretchToFit" VALUE="-1">';
	str_object		+= '		<PARAM NAME="windowlessVideo" VALUE="0">';
	str_object		+= '		<PARAM NAME="enabled" VALUE="1">';
	str_object		+= '		<PARAM NAME="fullScreen" VALUE="0">';
	str_object		+= '		<PARAM NAME="enableErrorDialogs" VALUE="0">';
	str_object		+= '		<PARAM NAME="ShowStatusBar" VALUE="1">';
	str_object		+= '			<embed type="application/x-mplayer2" id="winMediaPlayerID" pluginspage="http://www.microsoft.com/Windows/MediaPlayer/" ';
	str_object		+= '				src="'+ radio_file +'" ';
	str_object		+= '				name=MediaPlayerTV ';
	str_object		+= '				width="420"';
	str_object		+= '				height="320"';
	str_object		+= '				AutoSize="1" ';
	str_object		+= '				AutoStart="1" ';
	str_object		+= '				ClickToPlay="1" ';
	str_object		+= '				DisplaySize="1" ';
	str_object		+= '				EnableContextMenu="1" ';
	str_object		+= '				EnableFullScreenControls="1" ';
	str_object		+= '				EnableTracker="1" ';
	str_object		+= '				Mute="0" ';
	str_object		+= '				PlayCount="1" ';
	str_object		+= '				ShowControls="1" ';
	str_object		+= '				ShowAudioControls="1" ';
	str_object		+= '				ShowDisplay="0" ';
	str_object		+= '				ShowGotoBar="0" ';
	str_object		+= '				ShowPositionControls="1" ';
	str_object		+= '				ShowStatusBar="1" ';
	str_object		+= '				ShowTracker="1">';
	str_object		+= '			</embed>';
	str_object		+= '	</OBJECT>';
	document.getElementById('Div_RadioObject').innerHTML	= str_object;

	var str_url		= document.getElementById('TD_URL').innerHTML;
	str_url			= str_url.replace(/sSubTitleBold/g, 'sSubTitle');
	document.getElementById('TD_URL').innerHTML		= str_url;
	document.getElementById(link_id).className		= 'sSubTitleBold';
}

var flag_forum	= -1;
var flag_blog	= -1;
function show_code_forum(num, title, u_view){
	if ( num == flag_forum ){
		document.getElementById('Div_Forum').style.display	= 'none';
		flag_forum	= -1;
	}
	else{
		var
		str_content	= '[CENTER]\n[url='+ u_view +'][IMG]http://radio.vuontinhnhan.net/images/VTNradio_dot_Net.jpg[/IMG][/url]\n';
		str_content	+= '[url='+ u_view +'][B]VTN Radio S·ªë 42: Ng√¥i nh√† h·ªìng[/B][/url]\n';
		str_content	+= '[url='+ u_view +'][B]'+ title +'[/B][/url]\n[/CENTER]\n';
		document.getElementById('Div_ForumTitle').innerHTML	= title;
		document.getElementById('ForumCode').value			= str_content;
		document.getElementById('Div_Forum').style.display	= '';
		flag_forum	= num;
	}

	document.getElementById('Div_Blog').style.display	= 'none';
	flag_blog	= -1;
}

function show_code_blog(num, title, file_path, u_view){
	if ( num == flag_blog ){
		document.getElementById('Div_Blog').style.display	= 'none';
		flag_blog	= -1;
	}
	else{
		var str_content	= '<CENTER><br><br><a href="'+ u_view +'" target="_blank"><img src="http://radio.vuontinhnhan.net/images/VTNradio_dot_Net.jpg" border="0" title="Nghe online t·∫°i VTNradio.Net"></a><br>';
			str_content	+= '<object classid="clsid:d27cdb6e-ae6d-11cf-96b8-444553540000" width="300" height="20" codebase="http://fpdownload.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=8,0,0,0" >';
			str_content	+= '<param name="movie" value="http://radio.vuontinhnhan.net/mp3player.swf" />';
			str_content	+= '<param name="flashvars" value="file='+ file_path +'&autostart=false" />';
			str_content	+= '<embed src="http://radio.vuontinhnhan.net/mp3player.swf" width="300" height="20" flashvars="file='+ file_path +'&autostart=false" ';
			str_content	+= 'type="application/x-shockwave-flash" pluginspage="http://www.macromedia.com/go/getflashplayer" /></object><br>';
			str_content	+= '<a href="'+ u_view +'" target="_blank" style="FONT-FAMILY: Verdana, Arial, Helvetica, sans-serif; FONT-SIZE: 8pt; FONT-WEIGHT: bold; TEXT-DECORATION: none; line-height: 15pt;">VTN Radio S·ªë 42: Ng√¥i nh√† h·ªìng<br>' + title +'</a><br><br></CENTER>';

		document.getElementById('Div_BlogTitle').innerHTML	= title;
		document.getElementById('BlogCode').value			= str_content;
		document.getElementById('Div_Blog').style.display	= '';
		flag_blog	= num;
	}

	document.getElementById('Div_Forum').style.display	= 'none';
	flag_forum	= -1;
}
