						<script language="javascript">
							function ajax_tellfriend(the_form){
								var the_url			= the_form.action;
								var post_vars		= ajaxGetFormVars(the_form, "", "");
								ajaxPostURL(the_url, "_js_eval", post_vars);
								return false;
							}

							function show_tellfriend_form(){
								document.getElementById('TblTellfriendForm').style.display		= '';
								document.getElementById('DivErrorMsgTellfriend').innerHTML		= '';

								var the_form	= document.TELLFRIENDFORM;
								the_form.friend_name.value	= '';
								the_form.friend_email.value	= '';
							}
						</script>

