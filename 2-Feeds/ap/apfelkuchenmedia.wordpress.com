			jQuery('.user-login-input', lForm).blur(function() {
				if( jQuery('.user-login-input', lForm).val() ==  '') {
					jQuery('.userlogin-label span', lForm).show();
					jQuery('.userlogin-label span', lForm).fadeTo(100, 1);
