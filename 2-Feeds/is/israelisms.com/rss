            <guid isPermaLink="false"><![CDATA[http://israelisms.com/index.php?post_id=114502#]]></guid>
            <media:thumbnail url="http://assets.libsyn.com/item/1093227" />
            <pubDate>Fri, 28 Jul 2006 15:18:00 +0000</pubDate>
            <itunes:keywords />
            <itunes:subtitle />
