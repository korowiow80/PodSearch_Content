}

#base{
	width: 770px;
	margin: 0 auto;
	padding: 0;
	text-align: left;
	background-color: #5b85b0;
}

#container{
	margin: 0;
	padding: 0;
	width: 769px;
	height: 612px;
	text-align: center;
}

h1{
	padding-top: 20px;
	font-size: 24px;
	font-weight: bold;
	color: white;
}

h1 span{
	color: white;
	vertical-align: middle;
	font-weight: bold;
	font-size: 48px;
}

h2{
	text-align: center;
	padding: 0;
	padding-top: 100px;
	color: white;
	font-family: Helvetica , sans-serif;
	font-weight: bold;
	font-size: 60px;
}

p{
	text-align: center;
	font-size: 14px;
	margin: 0;
	padding: 0;
	color: white;
}

