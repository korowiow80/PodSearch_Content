function write_related (google_rs) {
	if (!google_rs) {
		return;
	}
	var google_num_links = google_rs.length;

	if (google_num_links > 15) {
		google_num_links = 16;
	}

	var key_src = '';
	
	var halfway = parseInt(google_num_links / 2 - 0.5,10);

	if (google_rs[0] && google_rs[0].source) {
		key_src = google_rs[0].source;
	} else {
		key_src = "GO";
	}

	related_txt_top		= '<span class="white">Related Searches:&nbsp;</span>';
	related_txt		= '<li class="rs_left_top"><span id="rs1-title">Related Searches</span></li>';
	related_txt_right 	= '<li class="rs_left_top"><span id="rs2-title">Related Searches</span></li>';
	
	for(i = 0; i < google_num_links; i++){

		if (google_rs[i] && google_rs[i].term != undefined) {
			if (google_rs[i].source) {
				key_src = google_rs[i].source;
			} else {
				key_src = "GO";
			}

			if (i < 4) {
				related_txt_top += '<a href="/?' + query_forceadult + '&q_src=RS_T_0_' + key_src + '&q=' + encodeURIComponent(google_rs[i].term) + '&tk=' + token + '" class="white hover_blue" target="_top">' + google_rs[i].term + '</a>&nbsp;&nbsp;';
			}

			if (lander == 1){
				if (i == (google_num_links - 1)) {
					related_txt += '<li class="rs_left_bottom" style="font-size:12px;font-weight:bold;text-align:left"><a href="/?' + query_forceadult + '&q_src=RS_L_0_' + key_src + '&q=' + encodeURIComponent(google_rs[i].term) + '&tk=' + token + '" target="_top">' + google_rs[i].term + '</a></li>';
				} else {
					related_txt += '<li class="rs_left_middle" style="font-size:12px;font-weight:bold;text-align:left"><a href="/?' + query_forceadult + '&q_src=RS_L_0_' + key_src + '&q=' + encodeURIComponent(google_rs[i].term) + '&tk=' + token + '" target="_top">' + google_rs[i].term + '</a></li>';
				}
			}

			if (lander == 2){ 
				// alert(google_num_links);
				if (i > halfway) {
					if (i == (google_num_links - 1)) {
						related_txt_right += '<li nowrap class="rs_right_bottom"><a style="font-size:12px;font-weight:bold" href="/?' + query_forceadult + '&q_src=RS_R_0_' + key_src + '&q=' + encodeURIComponent(google_rs[i].term) + '&tk=' + token + '" target="_top">' + google_rs[i].term + '</a></li>';
					} else {
						related_txt_right += '<li nowrap class="rs_right_middle"><a style="font-size:12px;font-weight:bold" href="/?' + query_forceadult + '&q_src=RS_R_0_' + key_src + '&q=' + encodeURIComponent(google_rs[i].term) + '&tk=' + token + '" target="_top">' + google_rs[i].term + '</a></li>';
					}
				} else {
					if (i == halfway) {
						related_txt += '<li nowrap class="rs_left_bottom"><a style="font-size:12px;font-weight:bold" href="/?' + query_forceadult + '&q_src=RS_L_0_' + key_src + '&q=' + encodeURIComponent(google_rs[i].term) + '&tk=' + token + '" target="_top">' + google_rs[i].term + '</a></li>';
					} else {
						related_txt += '<li nowrap class="rs_left_middle"><a style="font-size:12px;font-weight:bold" href="/?' + query_forceadult + '&q_src=RS_L_0_' + key_src + '&q=' + encodeURIComponent(google_rs[i].term) + '&tk=' + token + '" target="_top">' + google_rs[i].term + '</a></li>';
					}
				}
			}
		}
	}
}

var RSpopularCategories = '';

function write_portal (portal_keywords) {

	if (!portal_keywords) {
		return;
	}
	for (i = 0; i < 7; i++) {
		if (portal_keywords[i] && portal_keywords[i].term != undefined) {
			if (portal_keywords[i].token && portal_keywords[i].token && portal_keywords[i].token != undefined) {
				portal_token = portal_keywords[i].token;
			}

			RSpopularCategories += '<div class=cat><a class="popular_category" href="/?' + query_forceadult + '&q_src=PRT&q=' + encodeURIComponent(portal_keywords[i].term) + '&tk=' + portal_token + '">' + portal_keywords[i].term + '</a><br/></div><br>';
		}
	}

	if (!google_categories) {
		return;
	}

	portal_keywords = google_categories;
	popularCategories = '<table width=600 border=0 cellpadding=0 cellspacing=0><tr>';
	var k =0;
	for (i = 0; i < portal_keywords.length; i++) {

		if (portal_keywords[i] && portal_keywords[i].term != undefined) {
			if (portal_keywords[i].token && portal_keywords[i].token && portal_keywords[i].token != undefined) {
				portal_token = portal_keywords[i].token;
			}
			popularCategories += '<td align=left>';
			popularCategories += '<a class="cat" style="font-size:12px;font-weight:bold" href="/?' + query_forceadult + '&q_src=PRT&q=' + encodeURIComponent(portal_keywords[i].term) + '&tk=' + portal_token + '">' + portal_keywords[i].term + '</a><br>';
			
			if (portal_keywords[i].subcategories)  {
				var google_subcategories = portal_keywords[i].subcategories;
				popularCategories += '<ul>';
		
				for (var j = 0; j < google_subcategories.length; j++) {
					if (google_subcategories[j].term) {
						popularCategories += '<li><a class="cat" style="font-size:11px;font-weight:normal" href="/?' + query_forceadult + '&q_src=PRT&q=' + encodeURIComponent(google_subcategories[j].term) + '&tk=' + portal_token + '">' + google_subcategories[j].term + '</a>';

				}
				}
				popularCategories += '</ul></td>';
				k++;
				if (k == 3) {
					popularCategories += '</tr><tr>';
					k = 0;
				}
			}
		}
	}
	popularCategories += '</tr></table>';	

}

function write_ads (google_ads) {
	google_num_ads = google_ads.length;
         
	for(i = 0; i < google_num_ads; i++){
		mainads +='<div id="t1" style="padding-bottom:5px;min-width:300px;"><ul>';
		mainads +='<table border="0" cellspacing="1" cellpadding="0">';
		mainads +='<tr><td rowspan="100%" valign="top" style="width:35px; padding-top:5px;"><img src="' + img_srv + '/img/_marquee2/ads-arr-mq2.gif" width="24" height="24" alt=""></td></tr>';

		//mainads +='<tr><td><a class="ad_headline nodecoration hover_red" href="' + index + '?&p=' + i + '&q=' + encodeURIComponent(q) + '&q_src=' + encodeURIComponent(q_src) + '&track=' + encodeURIComponent(track) + '" target="_blank">' + google_ads[i].line1 + '</a></td></tr>';
		
		mainads +='<tr><td><a onClick="log_click(\'&q=' + encodeURIComponent(q) + '&q_src=' + encodeURIComponent(q_src) + '&track=' + encodeURIComponent(track) + '\');" class="ad_headline nodecoration hover_red" href="' + google_ads[i].url + '" target="_blank">' + google_ads[i].line1 + '</a></td></tr>';
		
		mainads +='<tr><td class="ad_line">' + google_ads[i].line2 + '</td></tr>';

		if (google_ads[i].seller_ratings) {

			advertiser_info = google_ads[i].seller_ratings.advertiser_info;
			rating 		= google_ads[i].seller_ratings.rating;
			review_count	= google_ads[i].seller_ratings.review_count;
			source_url 	= google_ads[i].seller_ratings.source_url;

			mainads += '<tr><td class="ad_line">' + advertiser_info + ' ';
			mainads += '<img src="' + img_srv + '/img/stars_one.png" width=9 height=9 border=0><img src="' + img_srv + '/img/stars_one.png" width=9 height=9 border=0><img src="' + img_srv + '/img/stars_one.png" width=9 height=9 border=0>';
						
			if (rating == "4") {
				mainads += '<img src="' + img_srv + '/img/stars_one.png" width=9 height=9 border=0><img src="' + img_srv + '/img/stars_zero.png" width=9 height=9 border=0>';
			}
			if (rating == "4.5") {
				mainads += '<img src="' + img_srv + '/img/stars_one.png" width=9 height=9 border=0><img src="' + img_srv + '/img/stars_half.png" width=9 height=9 border=0>';
			}
			if (rating == "5") {
				mainads += '<img src="' + img_srv + '/img/stars_one.png" width=9 height=9 border=0><img src="' + img_srv + '/img/stars_one.png" width=9 height=9 border=0>';
			}
			mainads += ' (<a class="site_link" href="' + source_url + '" target="_ratings">' + review_count + '</a>)</td></tr>';
		}
		
		mainads +='<tr><td><a id="a1" class="ad_link hover_red" onClick="log_click(\'&q=' + encodeURIComponent(q) + '&q_src=' + encodeURIComponent(q_src) + '&track=' + encodeURIComponent(track) + '\');"  href="' + google_ads[i].url + '" target="_blank">' + google_ads[i].visible_url + '</a></td></tr>';
		
		if (google_ads[i].sitelinks) {
			mainads +='<tr><td style="padding-top:10px; padding-left:15px">';
			mainads +='<table border=0 cellpadding=0 cellspacing=0><tr>';
			var k = 0;
			for(j = 0; j < google_ads[i].sitelinks.length; j++){

				sitelink_url	= google_ads[i].sitelinks[j].url;
				sitelink_text	= google_ads[i].sitelinks[j].link_text;

				mainads += '<td style="padding-right:16px"><a class="site_link hover_red" onClick="log_click(\'&s=' + j + '&q=' + encodeURIComponent(q) + '&q_src=' + encodeURIComponent(q_src) + '&track=' + encodeURIComponent(track) + '\');" href="' + sitelink_url + '" target="_blank">' + sitelink_text + '</a></td>';
				
				k++;
				if (k == 2) {
					mainads += '</tr><tr>';
					k = 0;

				}
			}
			mainads += '</tr></table>';
			mainads += '</td></tr>';
		}
		mainads +='</table>';
		mainads +='</ul></div>';
	}
}

function write_searches (google_ads) {
   
	mainads += '<br><div style="font-size:8pt; color:#666;font-weight:bold;margin-top: -10px;position:relative; z-index:30;padding-bottom:10px;">Web Searches</div><br><br>';

	for(i = 1; i < google_num_ads; i++){
		mainads +='<div id="t1" style="padding-bottom: 5px;min-width:300px;"><ul>';
		mainads +='<table border="0" cellspacing="2" cellpadding="0">';
		mainads +='<tr><td rowspan="4" valign="top" style="width:35px; padding-top:5px;"><img src="' + img_srv + '/img/_marquee2/ads-arr-mq2.gif" width="24" height="24" alt=""></td></tr>';
		mainads +='<tr><td><a class="ad_headline nodecoration hover_red" href="' + urls[i] + '" target="_blank" onClick="log_click(\'&q_src=DMZ&q=' + encodeURIComponent(q) + '&url=' + encodeURIComponent(urls[i]) + '\');">' + titles[i] + '</a></td></tr>';
		mainads +='<tr><td class="ad_line">' + desc[i] + '</td></tr>';
		mainads +='<tr><td><a id="a1" class="ad_link hover_red" href="' + urls[i] + '" onClick="log_click(\'&q_src=DMZ&q=' + encodeURIComponent(q) + '&url=' + encodeURIComponent(urls[i]) + '\');" target="_blank">' + visible_urls[i] + '</a></td></tr>';
		mainads +='</table></ul></div>';
	}
}
