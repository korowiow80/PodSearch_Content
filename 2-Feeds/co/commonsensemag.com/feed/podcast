function contact_owner(domain) {
	var width =500;
	var height=400;
	var URL = 'http://www.ndoverdrive.com/domain_contact.php?domain=' + domain;
	win = window.open(URL, '_contact_owner', 'toolbar=0,scrollbars=0,location=0,statusbar=0,menubar=0,resizable=0,width=' + width + ',height=' + height);
	if (win) {
		win.focus();
	}
}
