			function fieldFocus(val,id){
				if((val == "Name") || (val == "Email")){
					var nameField = document.getElementById(id);
					nameField.value = "";
				}
			}

			function fieldBlur(val,id){
				if(val == "" && id == "nameField"){
					var nameField = document.getElementById(id);
					nameField.value = "Name";
				}
				if(val == "" && id == "emailField"){
					var nameField = document.getElementById(id);
					nameField.value = "Email";
				}
			}
				
