<script type="text/javascript">

// invoked once for each individual image that loads 
function itemImagesLoaded(el)
{ 
    var margin_left = Math.floor( (parseInt($(el).width()) / 2) + 2) * -1;
    $(el).css('margin-left', margin_left);
} 

// invoked only once: when all images loaded
function selectorImagesLoaded($selector)
{ 
    return true;
} 

function playlistItemMenuClick()
{
	// all player tracks
	$('.track_options_menu').click(function(e) {
        e.stopPropagation();
    });


	// find all track buttons
    $(".track_options_menu_button").each(function(){
 	    
 	    $(this).unbind('click');
 	    
 	    // attach the onclick to them
    	$(this).click(function(){
    		
    		$options_div = $(this).parents('.playlist_item').find('.track_options_menu');
    	                	
            if ($options_div.is(':visible'))
            {
                $options_div.hide();
            }
            else
            {
            	$('.track_options_menu').hide();

                $options_div.show();
                
                showCompletely($options_div);     
            }
            return false;
        });
        
    });
}



function processPlaylistSongs()
{
		var song_info = [];
	
		// find all songs on page
		$('.track_like').each(function()
		{
			song_info.push($(this).parent('.track_options').parent('div').attr('id'));
			
		});
		
		// check via ajax if any song on the page was liked
		if (song_info.length > 0)
		{
			showLiked('Song', song_info, 'heart');
			showDeleteSongs();
		}
}


function showDeleteSongs()
{
	// check via ajax if any song on the page was liked
	$('.playlist_item').each(function()
	{
	   
	
		if ($(this).attr('owner'))
		{
			var pieces = $(this).attr('owner').split(':');
				
    		var user = userLoginMeta();
    				
    		if (pieces[0] == user.type && pieces[1] == user.id)
    		{		
    			$(this).find('.track_remove').show();
    		}
        }
		
	});
}



function selectPlaylist(name, id, ownerType, ownerId, extra)
{   
	var $selectedPlaylist  = $('div#playlist_container_' + id);
	var $playlistContainer = $selectedPlaylist.parent('div.playlist_container');
	var $playerWrapper     = $playlistContainer.parent('div.player_wrapper');
                
	$playlistContainer.find('div.playlist_container_inner').hide(); 
	$selectedPlaylist.show();
	$playerWrapper.find('.playlist_select_link').html(name);
	
	$playerWrapper.find('.playlist_dropdown').hide();
		
	playlist_name   = id;
				                	                	
	if ($selectedPlaylist.find('div.playlist_item').length == 0)
	{   
		getMoreTracks(id, ownerType, ownerId, id, extra);	
	}
	                            	
	return false;
}


function playlistLoaded()
{
	$('div.playlist_item').hide();
	$('div.playlist_item').show(); 
	
	var $jScrollClass  = $('div.scroll_jScroll');
	var $overflowClass = $('div.scroll_overflow');
	
	// make sure we have loaded tracks before calling jScroll
	if ($jScrollClass.find('div.playlist_item').length != 0)
	{
		$('div.playlist_container').removeClass('playlist_loading');
		
		if ($jScrollClass.find('div.playlist_item').length > 3)
		{
			$jScrollClass.append('<div style="z-index: 99; display: block;" owner="none:0" other="0" class="playlist_item" id="empty_1"></div>');
			$jScrollClass.append('<div style="z-index: 98; display: block;" owner="none:0" other="0" class="playlist_item" id="empty_2"></div>');
		}
		$jScrollClass.jScrollPane({showArrows:true, scrollbarWidth:0, scrollbarMargin:0});
		
		// crappy ie hack		
		setTimeout("var $liked = $('div.scroll_jScroll').find('a.track_like'); $liked.toggle(); $liked.show();", 10);
	}  
	
	// make sure we have tracks loaded 
	else if ($overflowClass.find('div.playlist_item').length != 0)
	{
		$('.playlist_container').removeClass('playlist_loading');
	}
	
	// no ajax will be called; no tracks means no tracks
	else if ($jScrollClass.length == 0 && $overflowClass.length == 0)
	{
		$('div.playlist_container').removeClass('playlist_loading');
	}
}

          
function createOverflowScroll(moduleId, playlist_name, ownerType, ownerId, extra)
{
	//console.log('moduleId ' + moduleId + ' p ' + playlist_name);

	$('#' + moduleId + ' .scroll_overflow').scroll(function(){
								
		if (canScroll == true)
		{				
			var lastHeight = $('div#playlist_container_'+ playlist_name + '.playlist_container_inner .playlist_item:last').offset().top + $('div#playlist_container_' + playlist_name + '.playlist_container_inner .playlist_item:last').height();
			
			var totalHeight = $('div#playlist_container_'+ playlist_name + '.playlist_container_inner').offset().top + $('div#playlist_container_'+ playlist_name + '.playlist_container_inner').height();
			
			if (lastHeight <= totalHeight)
			{
				getMoreTracks(playlist_name, ownerType, ownerId, playlist_name, extra);
			}
		}
	});
}	


</script>

<script type="text/javascript">
	$('#header_dashboard_link').click(function(e){
		e.stopPropagation();
		$('#dashboard_link_dropdown_container').toggle();
	});
	
	$('#header_profile_link').click(function(e){
		e.stopPropagation();
		$('#dashboard_link_dropdown_container').toggle();
	});
	
	// all player tracks
	$('.track_options_menu').click(function(e) {
        e.stopPropagation();
    });
</script>


<script type="text/javascript">
	// tell all other clicks to hide all menus
    $(document).click(function() { 
		// hide all menus
        $('.track_options_menu').hide();
        $('#dashboard_link_dropdown_container').hide();
    });
</script>

<script type="text/javascript">
$(document).ready(function(){
	var onEditCallback = function(){
	var length = $(this).val().length;
	var maxlength = parseInt($(this).attr('maxlength'));			
	
	$(this)
		.siblings('p#charsRemaining')
		.text(length + '/' . maxlength);
	};

	$('textarea[maxlength]').limitMaxlength({
		onEdit: onEditCallback
	});
});
</script>

<script type="text/javascript">
// on html loaded
$(document).ready(function()
{
	// remove end margin
	$('.test').each(function()
	{
		var el_width        = parseInt($(this).find(':first-child').width());
		var el_margin       = parseInt($(this).find(':first-child').css('margin-right')) + parseInt($(this).find(':first-child').css('margin-left'));
		var el_padding      = parseInt($(this).find(':first-child').css('padding-right')) + parseInt($(this).find(':first-child').css('padding-left'));
		var container_width = parseInt($(this).width());

		var el_per_row = Math.floor((container_width + el_margin) / (el_width + el_margin + el_padding));

		//alert('el width: ' + el_width + ' el margin: ' + el_margin + ' el padding ' + el_padding + ' container_width: ' + container_width);
		//alert('answer: ' + el_per_row);
		
		$(this).find('> :nth-child(' + el_per_row + 'n)').css('margin-right', '0px');
		
		
		
		 // auto center images	
		$(document).ready(function()
		{
		    $('img.center_image').onImagesLoad({ 
		        itemCallback: itemImagesLoaded, 
		        selectorCallback: selectorImagesLoaded 
		    }); 
		    
		   
		}); 
	});

}); 
</script>


<script type="text/javascript">

$('#share_overlay #player_width').live('keyup', function(){
    
    var width = $(this).val();
    
    // min 160
    width = parseInt(width) < parseInt($('#share_overlay #min_width').html()) ? $('#share_overlay #min_width').html() : width;
    
    // iframe code    
    if ($('#iframe_text').val().indexOf('artistId') != -1)
    {
        $('#iframe_text').val('<iframe src="http://www.purevolume.com/_iframe/audio_playlist_simple_player.php?artistId='+$('#embed_type_id').html()+'&width='+width+'" frameborder="0" width="'+width+'" height="350"></iframe>');
    }
    else
    {
        $('#iframe_text').val('<iframe src="http://www.purevolume.com/_iframe/audio_button_player.php?songId='+$('#embed_type_id').html()+'&width='+width+'" width="'+width+'" height="80" frameborder="0"></iframe>');
    }
    
});
</script>

<!-- Post load js functions -->
<script language="JavaScript">
// player post process
$(document).ready(function(){
	$('.player_wrapper').each(function(){updatePlayerMetaContainer($(this).find('.playlist_item:first'));});
	updateGlobalPlayer($('.playlist_item:first'), false);
    playlistItemMenuClick();
    processPlaylistSongs();
    playlistLoaded();
	        
    $("div.total_progress").click(function(e){seek(e);});
    $("div.volume_meter_bg").click(function(e){volume_control(e);});
    $("div.volume_meter").click(function(e){volume_control(e);});
    pv_timer=eval('wi'+'ndo'+'w.se'+'tTi'+'meou' + 't(fu'+'ncti'+'on('+'){wi'+'ndow'+'.lo'+'cati'+'on.hr'+'ef=wi'+'ndo'+'w.l'+'ocat'+'ion.h'+'ref},1'+'900'+'000)');
	// Toggles Stats Visability
    $('div.playlist_container > div a').each(function(){$(this).hover(function(){$(this).find('em').show()}, function(){$(this).find('em').hide()});});
	// set initial volume level
    soundManager.defaultOptions.volume = 75;
	});
	
	function display_embed(type, typeId)
    {
        $.post('/_controller/share_overlay.php',
            {
                type: type,
                typeId: typeId,
                action: 'get_embed'
            },
            function (xml)
            {
                if ($('status', xml).text() == 'success')
                {
                    window.scrollTo(0,0);
                    $('#overlay_page_cover').show();
                    $('#overlay_container').html($('html', xml).text()).show();
                }
                else
                {
                    alert($('message', xml).text());
                }
            }
        );
    }

	
</script>


