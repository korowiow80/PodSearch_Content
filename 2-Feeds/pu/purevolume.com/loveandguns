<script language="JavaScript">

$(document).ready(function()
{ 
    $("form#comments_form").submit(function()
    {
        $('#comments_form_container').hide();
        $('#comments_waiting').show();
        
        $.post("/_modules/_controller/comments_module.php",{
        comment: $("#comment_text").val(),
        type: $("#comment_type").val(),
        id: $("#comment_type_id").val(),
        action: "add"
        },  function(xml) 
            {            
                if ($("status",xml).text() == 'success')
                {
                    // comment approved
                    $("#comment_text").val('');
                    $($("html",xml).text()).insertBefore("#comment_module ul li:first").css('display','none').fadeIn(2000);
                }
                else if ($("status",xml).text() == 'success_submitted')
                {
                    // comment needs approval
                    $("#comment_success_message").show().fadeTo(5000, 1).fadeOut(1000);
                    $("#comment_text").val('');
                }
                else
                {    
                    // error
                    alert($("message",xml).text());
                }
                
                $('#comments_form_container').show();
                $('#comments_waiting').hide();
            });
            
            return false;
    });
});

</script>

	<div class="comment_submit_container">

        <div id="comments_form_container">
