function showMoreAlbums(more) {
$('#show_more'+more).html('<img src=http://images.fotki.com/progress.gif>');
$.ajax({
type: "POST",
data: {
ajax_more_albums:  more
},
success: function(data) {
$(data).insertAfter('#show_more'+more);
$('#show_more'+more).remove();
MoveAlbumUpDownSortInit();
},
error: function() {
$('#show_more'+more).html('There was an error retrieving data from server');
}
});
}
