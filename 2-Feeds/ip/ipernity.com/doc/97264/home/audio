<script type="text/javascript">
<!--



function signin() {

 var frame = $('signin-iframe');

 if ( frame.select('iframe')[0] )
 {
 var iframe = frame.select('iframe')[0];
 }
 else
 {
 var iframe = document.createElement("iframe");
 iframe.style.width="700px";
 iframe.style.height="330px";
 iframe.style.frameBorder="0";
 iframe.setAttribute('width','700');
 iframe.setAttribute('height','330');
 iframe.setAttribute('frameBorder','0');
 iframe.setAttribute('border','0');
 iframe.setAttribute('scrolling','no');
 iframe.style.overflow="hidden";
 iframe.style.border="0";
 frame.appendChild(iframe);
 }

 var from = ( window.location && window.location.pathname) ? window.location.pathname : '/';

 iframe.src= "/sso/form/?ctx=login&from="+encodeURIComponent(from);

 iframe.onload = function() { iframe.focus(); };

 var div = $('signin');
 if ( ! div.overlay )
 {
 div.overlay = new overlay();
 div.parentNode.removeChild(div);
 document.body.appendChild(div);
 }

 var p = document.viewport.getDimensions();
 div.style.position="absolute";
 div.style.top = "70px";
 div.style.left = (_pi(p.width/2)-350)+"px";

 if ( $('doc_noplayer') )
 {
 $('doc_player').style.display= "none";
 $('doc_noplayer').style.display= "block";
 }

 div.overlay.show();

 new Effect.Appear(div,{duration:0.2});

 Event.observe(window,'keypress',signin_escape);

 $('top') && $('top').scrollTo();

 return false;
}

function signin_close(el) {
 if(el) el.blur();

 Event.stopObserving(window,'keypress',signin_escape);

 var div = $('signin');
 div.overlay.hide();
 new Effect.Fade(div,{duration:0.2});
 if ( $('doc_noplayer') )
 {
 $('doc_player').style.display= "block";
 $('doc_noplayer').style.display= "none";
 }
 return false;
}

var signin_escape = function(ev) { if ( ev.keyCode == Event.KEY_ESC ) signin_close(); }.bindAsEventListener();



//-->
</script>  </div>

 
