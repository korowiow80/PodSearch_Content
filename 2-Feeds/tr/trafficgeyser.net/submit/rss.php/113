            <script type="text/javascript">
                var i = 1,timer;
                function blinkMsg(){
                    timer = setInterval('blink()', 500);
                }
                function blink() {

 
                    if (i%4 == 0) {
                        $('downote').hide();
                    } else {
                        $('downote').show();
                    }
 
                    i++;
                }
                function AjaxEditUserPref()
                {
	
                    ShowLoading('Updating Profile...');
                    new Ajax.Request('/Users/ajaxEditUserPref', {
                        method:'post',
                        parameters: {},
                        requestHeaders: {},
	
                        onSuccess: function(transport)
                        {
                            HideLoading();
                        }
                    });
                }
                function popup(windowname) {
                    blanket_size(windowname);
                    window_pos(windowname);
                    toggle('blanket');
                    toggle(windowname);
                    showDiamondPop();
                }

                if (window.attachEvent) window.attachEvent("onload", sfHover);

            </script>


