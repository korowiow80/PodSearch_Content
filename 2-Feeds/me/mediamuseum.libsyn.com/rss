            <media:thumbnail url="http://assets.libsyn.com/item/1103257" />
            <pubDate>Sat, 07 Aug 2010 15:52:00 +0000</pubDate>
			<itunes:keywords>money,horror,vampires,terry,max,buffer,bland</itunes:keywords>
