        if (typeof linktrackercookie != 'undefined' && linktrackercookie) {
            var ltarray = linktrackercookie.split("::");
            if (typeof ltarray[4] != 'undefined' && ltarray[4]) s.eVar39 = ltarray[4];
            if (typeof ltarray[0] != 'undefined' && ltarray[0]) s.eVar40 = ltarray[0];
            if (typeof ltarray[1] != 'undefined' && ltarray[1]) s.eVar41 = ltarray[1];
            if (typeof ltarray[2] != 'undefined' && ltarray[2]) s.eVar43 = ltarray[2].replace(/^\s+|\s+$/g, ' ');
            if (typeof ltarray[3] != 'undefined' && ltarray[3]) s.eVar42 = ltarray[3].replace(/^\s+|\s+$/g, ' ');
            //$j.cookie("linktrackercookie", "", { expires: -1 });
            Delete_Cookie('linktrackercookie', '/', '');
            //alert(ltarray[0] + "--" + ltarray[1] + "--" + ltarray[2] + "--" + ltarray[3] + "--" + ltarray[4]);
        }
        //});
        /********* END   Set the cookie for link tracker ****************/

        /************* DO NOT ALTER ANYTHING BELOW THIS LINE ! **************/
        var s_code = s.t(); if (s_code) document.write(s_code)//--></script>
