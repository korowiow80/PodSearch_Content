.footer-inner {
padding: 10px 20px 20px;
}
.footer-outer a {
color: #5b0000;
}
.footer-outer a:visited {
color: #080505;
}
.footer-outer a:hover {
color: #ffffff;
}
.footer-outer .widget h2 {
color: #ffffff;
}
/* Mobile
----------------------------------------------- */
html body.mobile {
