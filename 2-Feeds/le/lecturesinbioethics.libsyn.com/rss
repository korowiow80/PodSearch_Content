            <guid isPermaLink="false"><![CDATA[http://lecturesinbioethics.libsyn.com/index.php?post_id=637408#]]></guid>
            <media:thumbnail url="http://assets.libsyn.com/item/1125414" />
            <pubDate>Wed, 18 Aug 2010 15:10:00 +0000</pubDate>
            <itunes:explicit>clean</itunes:explicit>
