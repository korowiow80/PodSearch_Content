<script>
function twitter_callback_function( tweet ) {
    var name = tweet[0].user.name.split(' ', 1);
	var name = tweet[1].user.name.split(' ', 1);
	var name = tweet[0].user.name.split(' ', 1);
	
    $('#last_tweet').html(
        '<div class="me">' +
        '<a href="http://twitter.com/' +
        tweet[0].user.screen_name + '">' +
        '<img src="' + tweet[0].user.profile_image_url +
        '" align="left" width="40" border="0" hspace="10px"/> ' +
         name[0] + ' auf	 Twitter</a></div><div class="msg"><br><br><span id="ubtweet"><b>Letzer Tweet:</b></span><br>' +
		tweet[0].text + '</p></div>' 
    );
    $('#last_tweet').addClass('last_tweet_box');
}
</script>
 
