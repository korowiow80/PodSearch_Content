          if (!f) continue;
          if (f.elements[nam].checked) cc_click(nam);
        }
      }

      if (document.addEventListener) document.addEventListener('DOMContentLoaded',cc_load,false);
      */

      controls_checkbox_states=new Array();
      cc_block=new Array();
