      function cc_set_state(nam,state) {
        crs=controls_checkbox_states[nam];
        var idbase=nam+"_";
        for (var k=0;k<crs.length;k++) {
          var cr=crs[k];
          var e=document.getElementById(idbase+cr);
          if (cr==state) e.style.display=""; else e.style.display="none";
        }
      }

     function cc_find_form(e) {
        while (true) {
          var e=e.parentNode;
          if (!e) break;
          if (!e.tagName) continue;
          if (e.tagName.toLowerCase()=="form") return e;
        }
        return 0;
      }

      function cc_click(nam) {
        if (cc_block[nam]) return;

        var e=document.getElementById(nam+"_normal");
        var f=cc_find_form(e);
        if (!f) return;

        f.elements[nam].checked=!f.elements[nam].checked;

        if (f.elements[nam].checked) cc_set_state(nam,"checked"); else cc_set_state(nam,"normal");

        cc_block[nam]=true;
        f.elements[nam].onclick();
        cc_block[nam]=false;

      }

      function cc_hover(nam) {
        var e=document.getElementById(nam+"_normal");
        var f=cc_find_form(e);
        if (!f) return;

        if (!f.elements[nam].checked) {
          cc_set_state(nam,"hover");
        } else {
          cc_set_state(nam,"checkedhover");
        }
      }

      function cc_unhover(nam) {
        var e=document.getElementById(nam+"_normal");
        var f=cc_find_form(e);
        if (!f) return;

        if (!f.elements[nam].checked) {
          cc_set_state(nam,"normal");
        } else {
          cc_set_state(nam,"checked");
        }
      }

