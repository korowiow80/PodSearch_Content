SLIDES.add_slide(s);

s = new slide();
s.src =  "http://www.parisbypod.com/images/animated/realparis.gif";
s.text = unescape("");
s.link = "http://www.parisbypod.com/index-sign-up.php";
s.target = "";
s.attr = "";
s.filter = "";
SLIDES.add_slide(s);

s = new slide();
s.src =  "http://www.parisbypod.com/images/animated/transcripts.gif";
s.text = unescape("");
s.link = "http://www.parisbypod.com/index-sign-up.php";
s.target = "";
s.attr = "";
s.filter = "progid:DXImageTransform.Microsoft.Fade()";
SLIDES.add_slide(s);

s = new slide();
s.src =  "http://www.parisbypod.com/images/animated/ipod.gif";
s.text = unescape("");
s.link = "http://www.parisbypod.com/index-sign-up.php";
s.target = "";
s.attr = "";
s.filter = "progid:DXImageTransform.Microsoft.Fade()";
SLIDES.add_slide(s);

s = new slide();
s.src =  "http://www.parisbypod.com/images/animated/mystery.gif";
s.text = unescape("");
s.link = "http://www.parisbypod.com/index-sign-up.php";
s.target = "";
s.attr = "";
s.filter = "";
SLIDES.add_slide(s);



if (false) SLIDES.shuffle();

//-->
</SCRIPT>
