<script type="text/javascript">
		function checkHeader() {
			$.ajax({
				dataType: "html",
				url: "/nobrefresh/header/header.html",
				success: function(data)
						 {
						 	$("div[id*=ondaceroHeader]").html($(data).html());
						 },
				error: function()
					   {
					   //alert('error');
					   }
			});
		}
		checkHeader();
</script>
