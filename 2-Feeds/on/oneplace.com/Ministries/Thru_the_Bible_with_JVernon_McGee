    $(document).ready(function() {
        var SS = Salem.Shared;

        SS.ConvertTextToUrl("Terms Of Use", "http://www.oneplace.com/account/terms-of-service.html", false);
        SS.ConvertTextToUrl("Privacy Policy", "http://www.oneplace.com/account/privacy-policy.html", false);
        SS.ConvertTextToUrl("Get to know us", "http://www.oneplace.com/about-us.html", false);
        SS.ConvertTextToUrl("Support", "http://www.oneplace.com/help", false);
        SS.ConvertTextToUrl("Contact Us", "http://www.oneplace.com/contact-us.html", false);
        SS.ConvertTextToUrl("Link To Us", "http://www.oneplace.com/help/link-to-us.html", false);
        SS.ConvertTextToUrl("Broadcast With Us", "http://www.oneplace.com/broadcast-get-info.html", false);
        SS.ConvertTextToUrl("Advertise With Us", "http://www.salemwebnetwork.com/Advertise.aspx", true);

        $("#topShowsDropDown").click(function() {
            $("#TopShows").slideToggle();
        });
        
        $("#topTopicsDropDown").click(function() {
            $("#TopTopics").slideToggle();
        });
    });
</script>

