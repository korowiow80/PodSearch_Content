    $(document).ready(function() {
        Salem.OnePlace.Show.Details.Init(
            'http://www.oneplace.com/ministries/thru-the-bible-with-j-vernon-mcgee/listen/joshua-19-20-280148/purchase/create-account',
            'http://www.oneplace.com/ministries/thru-the-bible-with-j-vernon-mcgee/listen/joshua-19-20-280148/purchase/sign-in');

        var showPlayButton = true; // Only show if media is audio
    
        
            if (showPlayButton) {
                // Check for mobile devices
                if (!CheckForMobile())
                {
                    // replace the SEO link with the play button
                    var divContent = '<a class="popper PlayButton" href="http://www.oneplace.com/ministries/thru-the-bible-with-j-vernon-mcgee/player/joshua-19-20-280148.html" name="player" title="Launch popup audio player"><img alt="Play Now" border="0" src="http://media.salemwebnetwork.com/zcast/op/buttons/button_play.png" /></a>';
            
                    $('a.PlayButton').replaceWith(divContent);
                    $('.popper').popupWindow({ 
                        centerBrowser:1,
                        height:642, 
                        width:707,
                        windowName:'player'
                    }); 
                }
            }
            else {
                // remove the PlayButton div, as it is not needed.
                $('div.PlayNowButton').remove();
            }
        

        // if called from download button on the popup player, start the download reg process. // Todo: add rewrite rule /Download -> /Listen/#Download
        if (false || document.location.hash === "#download"){ 
            Salem.OnePlace.Show.Details.BeginDownload();
        } 
        
        // if called from purchase button on the popup player, start the purchase process.
        if (false || document.location.hash === "#download"){ 
            if (false){ 
                $("#DownloaderLink").click();
            }
            else 
            {
                $("#AccountQueryLink").click();
            }
        }
        
        // if called from the purchase success page, start the download popup
        if (false){ 
            $("#PurchaseSuccessDownloaderLink").click();
        } 

    });

    </script>

        </div>
        
        

