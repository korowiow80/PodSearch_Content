    function clickThroughOnClick(href, obj, itemId) {

        performAuditAction(itemId);

        window.open(href);

        return false;
    }

    function performAuditAction(itemId) {
        $.ajax({
            url: 'http://www.oneplace.com/ministries/showoffer/audit',
            type: "POST",
            data: { showOfferId: itemId },
            dataType: "json",
            async: false
        });
    }
