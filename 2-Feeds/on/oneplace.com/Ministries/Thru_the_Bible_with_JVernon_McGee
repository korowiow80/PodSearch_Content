        }
        else {
            $.blockUI.defaults.css = {};
            $.blockUI({ message: $('#FocusWindowSection') });

            $('#FocusWindowClose').live("click", function () {
                $.unblockUI();
                return false;
            });

            $('#DownloadInstructions').html(fullText + '<a class="PopupLink" href="http://www.oneplace.com/ministries/thru-the-bible-with-j-vernon-mcgee/listen/joshua-19-20-280148/direct-download/thru-the-bible-with-j-_2012-06-06_joshua-19-20_20120510002306.mp3"><img class="DownloadButton" src="http://media.salemwebnetwork.com/zcast/op/Buttons/download.png" alt="Download Episode" /></a>');
        }
