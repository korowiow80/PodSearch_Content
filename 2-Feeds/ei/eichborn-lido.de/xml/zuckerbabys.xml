<script type="text/javascript">

function loadPageVar (sVar) {  
	return unescape(window.location.search.replace(new RegExp("^(?:.*[&\\?]" + escape(sVar).replace(/[\.\+\*]/g, "\\$&") + "(?:\\=([^&]*))?)?.*$", "i"), "$1"));  
}
	
jQuery(document).ready(function(){

	// check search-attribute and hash-tag in URL
	if(window.location.search) {
		if(loadPageVar("luebbe-nl-ok")) {
			jQuery('#newsletter_ok').attr('src', 'http://www.luebbe.de/newsletter_ok.php');
			jQuery('#lightboxInner_NewsletterOk .CloseLightBox span.title').html('Newsletter-Anmeldung');
			jQuery(".topHeaderNewsletterOk").trigger("click");
		} else if(loadPageVar("luebbe-nl-fehler")) {
			jQuery('#newsletter_err').attr('src', 'http://www.luebbe.de/newsletter_fehler.php');
			jQuery('#lightboxInner_NewsletterErr .CloseLightBox span.title').html('Fehler bei Newsletter-Anmeldung');
			jQuery(".topHeaderNewsletterErr").trigger("click");
		} else if(loadPageVar("luebbe-nl-verwaltung")) {
						var email = "";
			var msgid = "";
			if (email.length > 6 && msgid.length > 3) {
				jQuery('#lightboxInner_NewsletterVerwaltung .CloseLightBox span.title').html('Newsletter verwalten');
				var iframe_source = 'http://www.nl-services.com/subscribe/bastei_luebbe/profil/profile.php?addr=' + email + '&msgid=' + msgid;
				var original_iframe_source = "http://www.nl-services.com/subscribe/bastei_luebbe/subscribe.php";
				jQuery('#newsletter_verwaltung').attr('src', iframe_source);
				jQuery(".topHeaderNewsletterVerwaltung").trigger("click");
			}
		}
  	}
	
	
	
	//check if hash tag exists in the URL
	if(window.location.hash) {

		 //set the value as a variable, and remove the #
		var newsletter_hash_value = window.location.hash.replace('#', '');

		if(newsletter_hash_value == "ok"){	
			jQuery('#newsletter_ok').attr('src', 'http://www.luebbe.de/newsletter_ok.php');
			jQuery('#lightboxInner_NewsletterOk .CloseLightBox span.title').html('Newsletter-Anmeldung');
			jQuery(".topHeaderNewsletterOk").trigger("click");
		}
		else if(newsletter_hash_value == "fehler"){	
			jQuery('#newsletter_err').attr('src', 'http://www.luebbe.de/newsletter_fehler.php');
			jQuery('#lightboxInner_NewsletterErr .CloseLightBox span.title').html('Fehler bei Newsletter-Anmeldung');
			jQuery(".topHeaderNewsletterErr").trigger("click");
		}
		else if(newsletter_hash_value != "ok" && newsletter_hash_value != "fehler"){	
						var email = "";
			var msgid = "";
			if (email.length > 6 && msgid.length > 3) {
				jQuery('#lightboxInner_NewsletterVerwaltung .CloseLightBox span.title').html('Newsletter verwalten');
				var iframe_source = 'http://www.nl-services.com/subscribe/bastei_luebbe/profil/profile.php?addr=' + email + '&msgid=' + msgid;
				var original_iframe_source = "http://www.nl-services.com/subscribe/bastei_luebbe/subscribe.php";
				jQuery('#newsletter_verwaltung').attr('src', iframe_source);
				jQuery(".topHeaderNewsletterVerwaltung").trigger("click");
			}
		}
	}

});
</script>
