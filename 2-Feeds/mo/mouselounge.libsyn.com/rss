            <itunes:explicit>clean</itunes:explicit>
			<itunes:keywords>podcast,disney,disneyland,walt,world</itunes:keywords>
            <itunes:subtitle><![CDATA[=--The Mouse Lounge Podcast--=]]></itunes:subtitle>
            <itunes:duration>1:39:05</itunes:duration>
